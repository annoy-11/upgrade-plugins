/* $Id:main.css 2015-4-1 00:00:00 SocialEngineSolutions $*/

.sesteam_icon_team:before{
	content:"\\f0c0";
	font-family: fontawesome;
}
/*Global css*/
.sesteam_label{
  color: #fff;
	display: inline-block;
	font-size: 10px;
	font-weight: bold;
	letter-spacing: 1px;
	margin:1px;
	padding: 3px 6px;
	text-align: center;
}
.sesteam_label_featured{background-color:#47c8e3;}
.sesteam_label_sponsored{background-color:#53dc10;}

/*WIGETS*/
[class*='layout_sesteam_']{
  clear:both;
  margin-bottom:15px;
}
[class*='tab_layout_sesteam_']{
  margin:0;
  clear:none;
}
/*Team Page:- Template 1 css start here*/
.sesteam_temp1_list .team_member_thumbnail{
  border:1px solid $theme_border_medium_color;
}
.sesteam_temp1_list .team_box_bottom{
  background-color:$theme_foreground_background_color;
}
.sesteam_temp1_list .team_box:hover .team_box_inner{
  background-color:$theme_body_background_color;
}
/*Team Page:- Template 1 css end here*/

/*Team Page:- Template 2 css start here*/
.sesteam_temp2_list .team_member_thumbnail{
  border:1px solid $theme_border_medium_color;
}
.sesteam_temp2_list .team_box:hover .team_member_thumbnail{
  border:1px solid $theme_border_dark_color;
}
.sesteam_temp2_list .team_member_name a{
  color:$theme_font_color;
}
.sesteam_temp2_list .team_member_info{
  border-top:1px dashed $theme_border_medium_color;
}
/*Team Page:- Template 2 css end here*/

/*Team Page:- Template 3 css start here*/
.sesteam_temp3_list .team_member_thumbnail_inner{
  border:1px solid $theme_border_medium_color;
}
.sesteam_temp3_list .team_member_stats{
  border-top:1px dashed $theme_border_medium_color;
}
/*Team Page:- Template 3 css end here*/

/*Team Page:- Template 4 css start here*/
.sesteam_temp4_list .team_member_thumbnail{
  border:1px solid $theme_border_medium_color;
}
.sesteam_temp4_list .team_box:hover .team_member_thumbnail{
  border:1px solid $theme_border_dark_color;
}
/*Team Page:- Template 1 css end here*/