/*CART ICON*/
.sesproduct_add_cart_dropdown,
.estore_my_wishlist_dropdown{
	position: relative;
}
.sesproduct_add_cart_dropdown.cart_icon:before{
	content:"\\f290";
	font-family: Fontawesome;
	font-size: 20px;
	padding: 0 5px;
}
.sesproduct_add_cart_dropdown.show_icon > span:not(.cart_value),
.sesproduct_add_cart_dropdown.show_text:before{
	display: none;
}
.sesproduct_add_cart_dropdown .cart_icon span.cart_value,
.sesproduct_add_cart_dropdown > span.cart_value,
.estore_my_wishlist_dropdown .wishlist_value{
	display: block;
  position: absolute;
  top: 0px;
  right: 0px;
  width: 14px;
  z-index: 99;
  background: #f00;
  height: 14px;
  font-size: 8px;
  border-radius: 50%;
  text-align: center;
  color: #fff;
  line-height: 14px;
}


.sesproduct_cart_dropdown{
	position: absolute;
	margin-top:10px;
	z-index: 999;
	min-height: 100px;
	min-width:300px;
}
.sesproduct_cart_dropdown img{
	display: block;
	margin:5px auto;
}
.sesproduct_cart_dropdown_inner {
  max-width: 100%;
  width: 235px;
  float: left;
  box-shadow: 0px 0px 10px rgba(0,0,0,0.1);
}
.sesproduct_cart_ditem,
.sesproduct_cart_Ftotal,
.sesproduct_cart_dfooter{
	display: flex;
	justify-content: space-between;
}
.sesproduct_cart_dhead{
	padding: 10px 15px;
}
.sesproduct_cart_dhead .total_products ._items{
	font-weight: bold;
	font-size: 90%;
	display: block;
	text-align: right;
}
.sesproduct_cart_dbody{
	padding:10px;
	max-height: 250px;
	overflow-y: auto;
}
.sesproduct_cart_dbody::-webkit-scrollbar{
  width: 3px;
  height: 5px;
}
.sesproduct_cart_dbody::-webkit-scrollbar-track{
  background: #fff; 
}
.sesproduct_cart_dbody::-webkit-scrollbar-thumb{
  background: #999; 
  border-radius: 10px;
}
.sesproduct_cart_dbody::-webkit-scrollbar-thumb:hover{
  background: #666; 
}
.sesproduct_cart_ditem{
	border-bottom: 1px solid #eee;
	padding: 5px 0;
	position: relative;
}
.sesproduct_cart_ditem:last-child{
	border:none;
}
.sesproduct_cart_dimg{
	width: 40px;
	height: 40px;
	margin-right: 5px;
}
.sesproduct_cart_dimg img{
	vertical-align: middle;
  width: 100%;
  height: 40px;
  margin:0 auto !important;
  object-fit: cover;
  border-width: 1px;
}
.sesproduct_cart_ddesc{
  width: 78%;
}
.sesproduct_cart_ddesc h3{
	font-size: 90%;
	text-overflow: ellipsis;
	white-space: nowrap;
	overflow: hidden;
	width: 90%;
	margin-bottom:3px !important;
	font-weight:bold;
	padding: 0;
}
.sesproduct_cart_ddesc p{
	margin:5px 0;
	font-size: 80%;
	display: flex;
	justify-content: space-between;
}
.sesproduct_cart_dclose{
	position: absolute;
	top:0;
	right:0;
}
.sesproduct_cart_Ftotal .total_value,
.sesproduct_cart_Ftotal .grand_total{
	padding: 10px;
}
.sesproduct_cart_Ftotal .total_value ._value,
.sesproduct_cart_Ftotal .grand_total span{
	font-weight: bold;
	font-size: 90%;
}
.sesproduct_cart_Ftotal .total_value span{
	display: block;
	text-align: right;
}
.sesproduct_cart_dfooter{
	padding:15px 10px;
}
.sesproduct_cart_dfooter a{
	padding: 4px 8px;
  font-size: 85%;
}
.sesproduct_cart_dfooter a.btn_border{
	border-width: 1px;
}

