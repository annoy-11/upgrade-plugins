	background-color:rgba(0, 0, 0, .6);
	border-radius:5px;
	margin-bottom:20px;
	padding:15px;
	margin:0 -15px;
}
.seslp_des5wid5_list_item{
	float:left;
	padding:15px;
	width:33.33%;
}
.seslp_des5wid5_list_item article{
	display:block;
	position:relative;
}
.seslp_des5wid5_list_item_thumb{
	overflow:hidden;
	position:relative;
}
