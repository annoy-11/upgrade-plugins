/*DESIGN 5 Start HERE*/
.seslp_des5wid_head{
	margin-bottom:50px;
}
.seslp_des5wid_head h2{
	float:left;
	font-size:36px;
	font-weight:500;
	padding:0;
	margin:0;
	letter-spacing:0;
}
.seslp_des5wid_head h2 span{
	color:#ff3600;
}
.seslp_des5wid_head_btn{
	float:right;
}
.seslp_des5wid_head_btn a{
	background-color:#ff3600;
	font-size:20px;
	border-radius:3px;
	color:#fff;
	padding:5px 20px 3px;
	margin-top:7px;
	display:block;
}
.seslp_des5wid_head_btn a:hover{
	opacity:.8;
	text-decoration:none;
}
/*Design5 Widget2 Start*/
.seslp_des5wid2_wrapper{
	background-color:#d7d7d7;
	padding:50px 0;
}
.seslp_des5wid2_listings{
	margin-bottom:20px;
	margin:0 -15px;
	display:flex;
	flex-wrap:wrap;
}
.seslp_des5wid2_list_item{
	margin:0 10px;
	width:calc(25% - 20px);
}
.seslp_des5wid2_list_item_overlay:before {
	content:"";
	position:absolute;
	border:4px double;
	width:90%;
	height:90%;
	margin:auto;
	display:block;
	left:0;
	right:0;
	top:0;
	bottom:0;
	transition:all .5s ease;
}
.seslp_des5wid2_list_item article:hover .seslp_des5wid2_list_item_overlay:before {
	opacity:0;
	transition:all .5s ease;
}
.seslp_des5wid2_list_item article{
	display:block;
	position:relative;
	overflow:hidden;
}
.seslp_des5wid2_list_item_thumb{
	background-position:center;
	background-size:cover;
	display:block;
	height:300px;
	filter:grayscale(1);
	transition:all .5s ease;
}
.seslp_des5wid2_list_item_overlay{
	background-color:rgba(0, 0, 0, .4);
	position:absolute;
	left:0;
	top:0;
	right:0;
	bottom:0;
	z-index:1;
}
.seslp_des5wid2_list_item_cont{
	position:absolute;
	left:0;
	top:0;
	right:0;
	bottom:0;
	text-align:center;
	padding:0 30px;
	padding-top:50%;
}
.seslp_des5wid2_list_item_cont *{
	color:#fff !important;
	position:relative;
	z-index:2;
}
.seslp_des5wid2_list_item_cont > span{
	display:block;
}
.seslp_des5wid2_list_item_owner{
	font-size:14px;
}
.seslp_des5wid2_list_item_title{
	font-size:20px;
	font-weight:500;
	white-space:nowrap;
	overflow:hidden;
	text-overflow:ellipsis;
}
.seslp_des5wid2_list_item_cont_info{
	text-align:center;
	opacity:0;
	visibility:hidden;
}
.seslp_des5wid2_list_item_stats{
	margin-bottom:5px;
}
.seslp_des5wid2_list_item_stats span + span{
	margin-left:5px;
}
.seslp_des5wid2_list_item_stats span i{
	margin-right:5px;
}
.seslp_des5wid2_list_item article:hover .seslp_des5wid2_list_item_thumb{
	transform:scale(1.05);
	filter:grayscale(0);
}
.seslp_des5wid2_list_item article:hover .seslp_des5wid2_list_item_cont{
	padding-top:45%;
}
.seslp_des5wid2_list_item article:hover .seslp_des5wid2_list_item_cont_info{
	opacity:1;
	visibility:visible;
}
/*Design5 Widget2 Ends*/
