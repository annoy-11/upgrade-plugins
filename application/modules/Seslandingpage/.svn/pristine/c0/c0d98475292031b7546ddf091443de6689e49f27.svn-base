	background-position:center;
	background-size:cover;
	display:block;
	height:260px;
}
.seslp_des5wid5_list_item_thunb_overlay{
	background-color:rgba(0, 0, 0, 0);
	position:absolute;
	left:0;
	top:0;
	right:0;
	bottom:0;
}
.seslp_des5wid5_list_item_thunb_overlay i{
	border-radius:50%;
	border:1px solid #fff;
	position:absolute;
	left:50%;
	top:50%;
	transform:translate(-50%,-50%);
	height:80px;
	width:80px;
	text-align:center;
	opacity:0;
}
.seslp_des5wid5_list_item_thunb_overlay i:before{
	font-size:30px;
	color:#fff;
	line-height:80px;
}
.seslp_des5wid5_list_item article:hover .seslp_des5wid5_list_item_thumb_img{
	transform:scale(1.05);
}
.seslp_des5wid5_list_item article:hover .seslp_des5wid5_list_item_thunb_overlay{
	background-color:rgba(0, 0, 0, .6);
}
.seslp_des5wid5_list_item article:hover .seslp_des5wid5_list_item_thunb_overlay i{
