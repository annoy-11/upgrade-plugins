/*DESIGN 4 STARTS HERE*/
/*Design4 Widget2 Start*/
.seslp_des4wid2_wrapper{
	padding:50px 0;
}
.seslp_des4wid2_features_list{
	margin:0 -15px;
}
.seslp_des4wid2_feature_item{
	float:left;
	margin-top:70px;
	padding:0 15px;
	width:33.33%;
}
.seslp_des4wid2_feature_item article{
	border:1px solid #ffde00;
	padding:40px 30px 20px 30px;
	text-align:center;
	border-bottom:5px solid #ffde00;
	position:relative;
}
.seslp_des4wid2_feature_item_icon{
	color:#000;
	position:absolute;
	top:-36px;
	left:0;
	right:0;
	margin:0 auto;
	display:inline-block;
	width:70px;
	height:70px;
	line-height:116px;
	text-align:center;
	overflow:hidden;
	-webkit-transform:rotate(45deg);
	-moz-transform:rotate(45deg);
	-ms-transform:rotate(45deg);
	-o-transform:rotate(45deg);
	transform:rotate(45deg);
	transition:all .5s ease;
}
.seslp_des4wid2_feature_item_icon i{
	font-size:35px;
	-webkit-transform:rotate(-45deg);
	-moz-transform:rotate(-45deg);
	-ms-transform:rotate(-45deg);
	-o-transform:rotate(-45deg);
	transform:rotate(-45deg);
	/*-webkit-transition:opacity 0.5s, -webkit-transform 0.5s;
	-moz-transition:opacity 0.5s, -moz-transform 0.5s;
	transition:all 0.5s, transform 0.5s;*/
	background-repeat: no-repeat;
	width: 40px;
	background-size: 100%;
	height: 100%;
	position: absolute;
	left: 0;
	right: 0;
	margin: auto;
	background-position: center center;
}
.seslp_des4wid2_feature_item_cont{
	text-align:center;
	margin:25px 0 10px;
}
.seslp_des4wid2_feature_item_cont h3{
	margin:0;
	padding:0;
	font-size:24px;
}
.seslp_des4wid2_feature_item_cont p{
	font-size: 15px;
	letter-spacing: .5px;
}
.seslp_des4wid2_head {
	text-align: center;
}
.seslp_des4wid2_head h2 {
	font-size: 34px;
	display: inline-block;
}
.seslp_des4wid2_des {
	text-align: center;
	font-size: 16px;
	letter-spacing: .5px;
}
/*Design4 Widget2 End*/
/*Design4 Widget3 Start*/
.seslp_des4wid3_wrapper{
	padding:0;
}
.seslp_des4wid3_column{
	float:left;
	width:50%;
}
.seslp_des4wid3_item{
	float:left;
	height:160px;
	position:relative;
	overflow:hidden;
	width:25%;
}
.seslp_des4wid3_item_thumb,
.seslp_des4wid3_item_thumb > span{
	background-size:cover;
	background-position:center;
	display:block;
	height:100%;
	width:100%;
}
.seslp_des4wid3_item:hover .seslp_des4wid3_item_thumb > span{
	transform:scale(1.02);
}
.seslp_des4wid3_item_thumb:after{
	content:"";
	background-color:rgba(0, 0, 0, .3);
	position:absolute;
	left:0;
	top:0;
	height:100%;
	width:100%;
	-webkit-transition:all 800ms ease 0s;
	-moz-transition:all 800ms ease 0s;
	-o-transition:all 800ms ease 0s;
	transition:all 800ms ease 0s;
}
.seslp_des4wid3_item:hover .seslp_des4wid3_item_thumb:after{
	background-color:rgba(0, 0, 0, .1);
}
.seslp_des4wid3_item_thumb > p{
	color:#fff;
	position:absolute;
	left:0;
	width:100%;
	text-align:center;
	z-index:1;
	font-size:20px;
	top:50%;
	transform:translateY(-50%);
}
.seslp_des4wid3_item_thumb > p i{
	margin-left:5px;
}
.seslp_des4wid3_item_big{
	height:300px;
	width:100% !important;
}
.seslp_des4wid3_item_icon{
	color:#fff;
	position:absolute;
	right:30px;
	bottom:30px;
	border:2px solid #fff;
	border-radius:50%;
	height:90px;
	width:90px;
	z-index:1;
}
.seslp_des4wid3_item_icon:before{
	font-size:40px;
	position:absolute;
	left:50%;
	top:50%;
	transform:translate(-50%,-50%);
}
/*Design4 Widget3 End*/

/*Design4 Widget4 Start*/
.seslp_des4wid4_wrapper{
	position:relative;
}
.seslp_des4wid4_wrapper:before{
	content:"";
	background-image:url(../images/transprant-bg.png);
	width:50%;
	position:absolute;
	right:0;
	top:0;
	height:100%;
}
.seslp_des4wid4_column{
	float:left;
	position:relative;
	width:50%;
}
.seslp_des4wid4_column_l{
	padding:50px 20px 50px 0;
}
.seslp_des4wid4_column_l h2{
	border-radius:1px;
	border:3px solid #ffde00;
	font-size:24px;
	display:inline-block;
	padding:5px 15px 2px;
	margin:0 0 30px;
}
.seslp_des4wid4_column_l h3{
	background:none;
	border-width:0;
	font-size: 42px;
	padding:0 20px 0 0;
	line-height:60px;
	margin:0 0 20px;
}
.seslp_des4wid4_column_l p{
	font-size:20px;
}
.seslp_des4wid4_column_l_btn{
	margin-top:40px;
	text-align:right;
}
.seslp_des4wid4_column_l_btn a{
	background-color:#ffde00;
	font-size:20px;
	color:#000;
	padding:5px 20px 2px;
	display:inline-block;
}
.seslp_des4wid4_column_l_btn a:hover{
	opacity:.8;
	text-decoration:none;
}
.seslp_des4wid4_column_r{
	padding:0 0 50px 20px;
}
.seslp_des4wid4_listing{
	margin:0 -15px;
}
.seslp_des4wid4_list_item{
	margin-top:50px;
	padding:0 15px;
	float:left;
	width:50%;
}
.seslp_des4wid4_list_item_thumb{
	height:200px;
	position:relative;
	overflow:hidden;
}
.seslp_des4wid4_list_item_img,
.seslp_des4wid4_list_item_img > span{
	background-size:cover;
	background-position:center;
	display:block;
	height:100%;
	width:100%;	
}
.seslp_des4wid4_list_item_thumb:hover .seslp_des4wid4_list_item_img > span{
	transform:scale(1.03);
}
.seslp_des4wid4_list_item_thumb_overlay{
	position:absolute;
	left:0;
	top:0;
	background-color:rgba(0, 0, 0, .1);
	opacity:0;
	filter:alpha(opacity=0);
}
.seslp_des4wid4_list_item_thumb:hover .seslp_des4wid4_list_item_thumb_overlay{
	opacity:1;
	filter:alpha(opacity=100);
}
.seslp_des4wid4_list_item_cat{
	position:absolute;
	bottom:15px;
	left:15px;
}
.seslp_des4wid4_list_item_cat a{
	background-color:#ffde00;
	color:#000;
	display:block;
	padding:0 10px;
}
.seslp_des4wid4_list_item_cat a:hover{
	opacity:.8;
}
.seslp_des4wid4_list_item_cont{
	padding-top:15px;
}
.seslp_des4wid4_list_item_title{
	font-size:22px;
}
.seslp_des4wid4_list_item_stats span + span{
	margin-left:8px;
}
.seslp_des4wid4_list_item_stats span i{
	margin-right:5px;
}
/*Design4 Widget4 End*/
/*Design4 Widget5 Starts*/
.seslp_des4wid5_container{
	background-color:#000;
	height:300px;
	text-align:center;
	position:relative;
	overflow:hidden;
	width:100%;
}
.seslp_des4wid5_members_listings{
	position:absolute;
	display:flex;
	flex-wrap:wrap;
	overflow-wrap:break-word;
}
.seslp_des4wid5_members_listings > a,
.seslp_des4wid5_members_listings > img{
	height:150px;
	width:150px;
	flex-grow:1;
	opacity:.4;
}
.seslp_des4wid5_members_listings > a:hover,
.seslp_des4wid5_members_listings > img:hover{
	opacity:1;
	object-fit:cover;
	transform:scale(1.1);
	z-index:2;
}
.seslp_des4wid5_members_listings > a > img{
	height:100%;
	object-fit:cover;
	width:100%;
}
.seslp_des4wid5_content{
	letter-spacing:2px;
	position:absolute;
	top:50%;
	transform:translateY(-50%);
	text-align:center;
	width:100%;
	z-index:9;
}
.seslp_des4wid5_content h2{
	color:#fff;
	font-size:32px;
}
.seslp_des4wid5_btns a{
	border:2px solid #fff;
	border-radius:5px;
	color:#fff !important;
	display:inline-block;
	font-size:16px;
	font-weight:600;
	margin:0 5px;
	padding:5px 20px;
	text-decoration:none !important;
}
.seslp_des4wid5_btns a:hover{
	background-color:#fff;
	color:#000 !important;
}
/*Design4 Widget5 Ends*/
/*Design4 Widget6 Starts*/
.seslp_des4wid6_wrapper{
	padding:50px 0;
}
.seslp_des4wid6_wrapper h2{
	font-size:38px;
	letter-spacing:normal;
	text-align:center;
	position:relative;
	padding:0 0 10px;
	margin:0 0 20px;
}
.seslp_des4wid6_wrapper h2:after{
	border-bottom:4px solid #ffde00;
	bottom:0;
	content:"";
	left:0;
	right:0;
	margin:auto;
	position:absolute;
	width:100px;
}
.seslp_des4wid6_listings{
	float:left;
	margin-top:30px;
	width:100%;
}
.seslp_des4wid6_listing_item{
	float:left;
	padding:0 15px;
	width:33.33%;
}
.seslp_des4wid6_listing_item article{
	box-shadow:0 0 10px rgba(0, 0, 0, .1);
}
.seslp_des4wid6_listing_item_thumb{
	height:250px;
	overflow:hidden;
	position:relative;
	width:100%;
}
.seslp_des4wid6_listing_item_thumb a,
.seslp_des4wid6_listing_item_thumb_img{
	background-position:center;
	background-size:cover;
	display:block;
	width:100%;
	height:100%;
}
.seslp_des4wid6_listing_item_thumb:hover .seslp_des4wid6_listing_item_thumb_img{
	transform:scale(1.1);
}
.seslp_des4wid6_listing_item_thumb_overlay{
	background-color:rgba(0, 0, 0, .6);
	position:absolute;
	left:0;
	top:0;
	bottom:0;
	right:0;
	opacity:0;
}
.seslp_des4wid6_listing_item_thumb:hover .seslp_des4wid6_listing_item_thumb_overlay{
	opacity:1;
	right:20px;
	left:20px;
	bottom:20px;
	top:20px;
}
.seslp_des4wid6_listing_item_thumb_overlay i{
	border-radius:50%;
	background-color:#ffde00;
	height:50px;
	width:50px;
	text-align:center;
	font-size:20px;
	position:absolute;
	top:50%;
	left:50%;
	color:#fff;
	margin:-25px 0 0 -25px;
}
.seslp_des4wid6_listing_item_thumb_overlay i:before{
	line-height:48px;
}
.seslp_des4wid6_listing_item_info{
	background-color:#fff;
	padding:15px;
}
.seslp_des4wid6_listing_item_title{
	font-size: 22px;
	overflow:hidden;
	text-overflow:ellipsis;
	white-space:nowrap;
	margin-bottom: 5px;
}
.seslp_des4wid6_listing_item_date{
	font-size:15px;
	margin-bottom:10px;
}
.seslp_des4wid6_listing_item_date p i{
	margin-right:5px;
}
.seslp_des4wid6_listing_item_date span + span{
	margin-left:8px;
}
.seslp_des4wid6_listing_item_des{
	font-size:15px;
}
/*Design4 Widget6 Ends*/

@media (min-width: 980px) and (max-width:1050px){
	
}
@media (min-width: 768px) and (max-width:979px){
	.seslp_des4wid2_feature_item_cont h3{
		font-size:20px;
	}
}
@media (max-width:767px){
	.seslp_des4wid2_feature_item{
		width:100%;
		padding:15px;
	}
	.seslp_des4wid3_column{
		width:100%;
	}
	.seslp_des4wid4_column{
		width:100%;
	}
	.seslp_des4wid4_column_l{
		padding:50px 0px 0px;
	}
	.seslp_des4wid4_wrapper:before{
		content:'';
		background-color:transparent;
	}
	.seslp_des4wid4_column_l h2{
		margin:0 0 10px
	}
	.seslp_des4wid4_column_l h3{
		font-size:32px;
		margin:0px;
	}
	.seslp_des4wid4_column_l p{
		font-size:14px;
	}
	.seslp_des4wid4_column_l_btn{
		margin-top:10px;
	}
	.seslp_des4wid4_column_l_btn a{
		font-size:14px;
	}
	.seslp_des4wid4_column_r{
		padding:0px;
	}
	.seslp_des4wid4_listing{
		margin:15px -15px;
	}
	.seslp_des4wid4_list_item{
		margin:0px;
		padding:15px;
	}
	.seslp_des4wid5_members_listings > a, .seslp_des4wid5_members_listings > img{
		width:120px;
		height:120px;
	}
	.seslp_des4wid6_wrapper{
		padding:30px 0px;
	}
	.seslp_des4wid6_wrapper h2{
		font-size:28px;
		padding-top:0px;
		margin:0px 0px 10px;
	}
	.seslp_des4wid6_listing_item{
		width:100%;
		padding:15px 0px;
	}
	.seslp_des4wid6_listings{
		margin-top:10px;
	}
}
@media (max-width:599px){

}
@media (max-width:479px){
	.seslp_des4wid3_item{
		width:50%;
	}
	.seslp_des4wid4_column_l h2{
		font-size:18px;
	}
	.seslp_des4wid4_column_l h3{
		line-height:30px;
		font-size:24px;
	}
	.seslp_des4wid4_list_item{
		width:100%;
	}
	.seslp_des4wid5_members_listings > a, 
	.seslp_des4wid5_members_listings > img{
		height:80px;
		widows:80px;
	}
	.seslp_des4wid5_content h2{
		font-size:24px;
	}
	.seslp_des4wid5_btns a{
		font-size:16px;
	}
	.seslp_des4wid6_wrapper h2{
		margin:0px;
		padding:0px 0 20px;
	}
}
/*DESIGN 4 ENDS HERE*/

