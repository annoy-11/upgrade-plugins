@import url('https://fonts.googleapis.com/css?family=Roboto+Condensed:400,400i,700,700i');
*, [class*="layout_seslandingpage_"] * {
	font-family: 'Roboto Condensed', sans-serif;
}
.fa {
	font-family: "FontAwesome" !important;
}
#global_wrapper{
	padding-top:0 !important;
}
#global_page_core-index-index {
	background: #f3f3f3;
}
#global_footer{
	margin-top:0 !important;
}
.sesadvheader_minimenu_login > a,.sesadvheader_minimenu_signup > a{
	color:white !important;
}
.layout_seslandingpage_landing_page {
	background: transparent !important;
}
.sesadvheader_main_menu > ul > li > a:hover, .sesadvheader_main_menu > ul > li.active a{
	color:white !important;
}
.seslandingpage_intro {
	position: relative;
	padding: 150px 0 80px;
	background-repeat: no-repeat;
	background-size: cover;
	background-position: top center;
	background-attachment: fixed;
}
.seslandingpage_intro:before {
	content: "";
	position: absolute;
	top: 0;
	left: 0;
	right: 0;
	bottom: 0;
	opacity: .9;
}
.seslandingpage_intro:after {
	content: "";
	position: absolute;
	top: 210px;
	left: -160px;
	right: 0;
	bottom: 0;
	background: url(/application/modules/Seslandingpage/externals/images/landingpage/oval-big-left.png);
	background-repeat: no-repeat;
	width: 100%;
	background-size: 40%;
}
.seslandingpage_intro .seslandingpage_intro_inner {
	max-width: 1200px;
	margin: 0 auto;
	width: 100%;
	text-align: center;
	z-index: 1;
	position: relative;
}
.seslandingpage_intro .seslandingpage_intro_inner h2 {
	font-size: 60px;
	font-weight: bold;
	color: white;
	position: relative;
}
.seslandingpage_intro .seslandingpage_intro_inner p {
	font-size: 25px;
	color: #c1c1c1;
	position: relative;
}
.seslandingpage_temp {
	padding: 60px 0 30px;
	background: #f9f9f9;
}
.seslandingpage_temp .seslandingpage_temp_inner {
	text-align: center;
	max-width: 1200px;
	margin: 0 auto;
	width: 100%;
}
.seslandingpage_temp .seslandingpage_temp_inner h3, .seslandingpage_features .seslandingpage_features_inner h3 {
	font-size: 45px;
	font-weight: bold;
}
.seslandingpage_temp .seslandingpage_temp_inner p, .seslandingpage_features .seslandingpage_features_inner p {
	font-size: 18px;
	margin-bottom: 30px;
}
.seslandingpage_temp .seslandingpage_demo ul {
	display: flex;
	flex-wrap: wrap;
}
.seslandingpage_temp .seslandingpage_demo ul li {
	width: calc(33% - 20px);
	margin: 10px;
	height: 300px;
	box-shadow: 0 11px 35px rgba(0,0,0,0.05);
	transition: all .5s ease;
	margin-bottom: 30px;
}
.seslandingpage_temp .seslandingpage_demo ul li a img {
	width: 100%;
	height: 100%;
}
.seslandingpage_temp .seslandingpage_demo ul li:hover {
	opacity: .8;
	transition: all .5s ease;
}
/* header */
.mini_menu_section:before {
	background: transparent !important;
}
.header_top {
	position: fixed;
	left: 0;
	right: 0;
	z-index: 9;
}
.mini_menu_section > ul > li.sesadvheader_minimenu_signup a, .mini_menu_section > ul > li.sesadvheader_minimenu_login a {
	padding: 5px 15px;
}
.sesadvheader_main_menu > ul > li.active a {
	color: #fff !important;
}
.seslandingpage_features {
	padding: 60px 0;
}
.seslandingpage_features .features_main {
	display: flex;
	flex-wrap: wrap;
}
.seslandingpage_features .features_main .features_box {
	width: calc(30% - 40px);
	margin: 10px;
	text-align: center;
	padding: 40px 25px;
	transition: all .5s ease;
}
.seslandingpage_features .features_main .features_box h4 {
	font-size: 20px;
	margin-bottom: 10px;
	position: relative;
}
.seslandingpage_features .features_main .features_box p {
	font-size: 15px;
	margin-bottom: 0;
	color: grey;
}
.seslandingpage_features .features_main .features_box:hover {
	transform: scale(1.04);
	transition: all .5s ease;
	box-shadow: 0 20px 80px rgba(0,0,0,0.12);
}
.seslandingpage_about .seslp_about_inner {
	max-width: 1200px;
	width: 100%;
	margin: 0 auto;
	display: flex;
	flex-wrap: nowrap;
}
.seslandingpage_about .about_img {
	width: 50%;
}
.seslandingpage_about .about_img img {
	margin: auto;
	display: block;
	box-shadow: rgba(15, 29, 51, 0.2) 0px 0px 80px 0px;
}
.seslandingpage_about {
	padding: 70px 0;
	overflow: hidden;
}
.seslandingpage_about .about_content {
	width: 50%;
	color: white;
	padding: 40px;
	margin-left: -130px;
	height: 190px;
	margin-top: 25px;
}
.seslandingpage_about .about_content h3 {
	font-size: 32px;
	letter-spacing: .5px;
	font-weight: bold;
	margin-bottom: 10px;
}
.seslandingpage_about .about_content p {
	font-size: 16px;
	letter-spacing: .5px;
	width: 85%;
	color: #c1c1c1;
}
.seslandingpage_about .about_content a {
	padding: 12px 25px;
	display: inline-block;
	font-size: 15px;
	text-transform: uppercase;
	border-radius: 30px;
	margin-top: 15px;
	margin-right: 10px;
	border: 1px solid;
	cursor: pointer;
}
.seslandingpage_about .about_content a:last-child {
	background: transparent !important;
	color: white !important;
}
.seslandingpage_intro .seslandingpage_intro_inner ul li {
	display: inline-block;
	width: 24%;
	margin-top: 70px;
}
.seslandingpage_intro .seslandingpage_intro_inner ul li img {
	width: 55px;
}
.seslandingpage_intro .seslandingpage_intro_inner ul li p {
	font-size: 16px;
}
.seslandingpage_intro .seslandingpage_intro_inner ul li h3 {
	font-size: 22px;
	color: white;
	margin: 10px 0 5px;
	letter-spacing: .5px;
}
.seslandingpage_intro .seslandingpage_intro_inner a {
	border: 1px solid;
	padding: 15px 30px;
	font-size: 18px;
	color: white;
	display: inline-block;
	margin-top: 25px;
	border-radius: 50px;
	transition:all .5s ease;
}
.seslandingpage_intro .seslandingpage_intro_inner a:hover{
	transition:all .5s ease;
}
.seslandingpage_intro .seslandingpage_intro_inner a i {
	margin-right: 10px;
}
.seslandingpage_parallax .seslandingpage_parallax_inner {
	max-width: 1200px;
	width: 100%;
	margin: auto;
	text-align: center;
}
.seslandingpage_parallax {
	background: url("/application/modules/Seslandingpage/externals/images/landingpage/bg.png");
	padding: 60px 0;
	background-attachment: fixed;
	background-blend-mode: overlay;
	background-color: #00000045;
}
.seslandingpage_parallax .seslandingpage_parallax_inner h3 {
	font-size: 45px;
	color: white;
	margin-bottom: 40px;
	font-weight: bold;
}
.seslandingpage_parallax .seslandingpage_parallax_inner p {
	font-size: 25px;
	width: 45%;
	margin: auto;
	color: white;
}
.seslandingpage_features .seslandingpage_features_inner {
	max-width: 1000px;
	width: 100%;
	margin: auto;
	text-align: center;
}
.seslandingpage_features .features_main .features_box img {
	width: 45px;
}
.seslandingpage_features .features_main .features_box h4:before {
	content: "";
	position: absolute;
	height: 2px;
	width: 15%;
	background: #b3b3b3;
	left: 0;
	right: 0;
	margin: auto;
	bottom: 0;
}
.seslandingpage_parallax .seslandingpage_parallax_inner ul li {
	display: inline-block;
	width: 33%;
	color: white;
	font-size: 16px;
	position: relative;
}
.seslandingpage_parallax .seslandingpage_parallax_inner ul li .img_icon {
	display: block;
	width: 100px;
	height: 100px;
	line-height: 100px;
	margin: 0 auto;
	border-radius: 100px;
	border: 1px dashed white;
	position: relative;
}
.seslandingpage_parallax .seslandingpage_parallax_inner ul li .img_icon img {
	width: 55%;
	vertical-align: middle;
}
.seslandingpage_parallax .seslandingpage_parallax_inner ul li .small_icon {
	position: absolute;
	top: 0;
	left: 70px;
	right: 0;
	margin: auto;
	background: white;
	width: 25px;
	height: 25px;
	line-height: 25px;
	border-radius: 20px;
}
.seslandingpage_parallax .seslandingpage_parallax_inner ul li .step_cont {
	width: 80%;
	display: block;
	margin: 10px auto;
}
.seslandingpage_parallax .seslandingpage_parallax_inner ul li:before {
	content: "";
	position: absolute;
	right: 20px;
	top: 20px;
	background: url(/application/modules/Seslandingpage/externals/images/landingpage/back-arrow.png);
	background-size: 15%;
	width: 100%;
	bottom: 0;
	background-repeat: no-repeat;
}
.seslandingpage_parallax .seslandingpage_parallax_inner ul li:first-child:before {
	display: none;
}
.seslandingpage_temp #view_less {
	display: none;
}
.seslandingpage_demo .view_button {
	display: block;
	padding: 20px;
	text-align: center;
	font-size: 18px;
	margin: 0 10px;
}
.seslandingpage_demo .view_button a {
	font-weight: 600;
	letter-spacing: 1px;
	border-radius:50px;
	padding: 10px 30px;
	transition:all .5s ease;
	cursor:pointer;
}
.seslandingpage_demo .view_button a:hover{
  color:white;
	transition:all .5s ease;
}
