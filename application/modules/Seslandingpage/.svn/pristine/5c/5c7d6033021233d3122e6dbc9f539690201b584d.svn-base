/*DESIGN 9 STARTS HERE*/
.seslp_des9_head h2{
	border-bottom-width:1px;
	font-size:32px;
	text-align:center;
	letter-spacing:normal;
	margin:0 0 50px;
	padding:0 0 20px;
	position:relative;
	font-weight:500;
}
.seslp_des9_head h2:after{
	content:"";
	background-color:#771e44;
	height:1px;
	bottom:-1px;
	position:absolute;
	left:50%;
	margin-left:-75px;
	width:150px;
}
/*Design9 Widget2 End*/
.seslp_des9wid2_wrapper{
	padding:50px 0;
}
.seslp_des9wid2_wrapper .seslp_blocks_container{
	border-width:1px;
	background-color:#fff;
	position:relative;
}
.seslp_des9wid2_wrapper .seslp_des9_head{
	padding:30px 50px 0;
}
.seslp_des9wid2_features{
	display:table;
	width:100%;
	margin-bottom:10px;
}
.seslp_des9wid2_feature_item{
	display:table-cell;
	padding:30px 20px;
	width:33.33%;
}
.seslp_des9wid2_feature_item + div{
	border-left-width:1px;
}
.seslp_des9wid2_feature_item_icon{
	border:1px solid #e4e4e4;
	position:relative;
	height:100px;
	width:100px;
	text-align:center;
	border-radius:50%;
	color:#771e44;
	font-size:40px;
	margin:0 auto 30px;
	line-height:80px;
	overflow:hidden;
	z-index:1;
}
.seslp_des9wid2_feature_item_icon:before{
	content:"";
	position:absolute;
	bottom:0;
	left:0;
	right:0;
	height:25px;
	background:#e4e4e4;
}
.seslp_des9wid2_feature_item_icon,
.seslp_des9wid2_feature_item_icon:before{
	-webkit-transition:all 800ms ease 0s;
	-moz-transition:all 800ms ease 0s;
	-o-transition:all 800ms ease 0s;
	transition:all 800ms ease 0s;
}
.seslp_des9wid2_feature_item_cont{
	text-align:center;
}
.seslp_des9wid2_feature_item_cont h3{
	background:none;
	border-width:0;
	padding:0;
	margin:0 0 5px;
	font-size:20px;
	font-weight:600;
	letter-spacing:normal;
}
.seslp_des9wid2_feature_item_cont p{
	font-size:16px;
}
.seslp_des9wid2_feature_item:hover .seslp_des9wid2_feature_item_icon{
	border-color:#771e44;
}
.seslp_des9wid2_feature_item:hover .seslp_des9wid2_feature_item_icon:before{
	background-color:#771e44;
}
/*Design9 Widget2 End*/

/*Design9 Widget3 Start*/
.seslp_des9wid3_wrapper{
	padding:50px 0;
	position:relative;
}
.seslp_des9wid3_bg{
	position:absolute;
	display:block;
	background-attachment:fixed;
	background-position:center;
	background-size:cover;
	height:100%;
	width:100%;
	left:0;
	top:0;
}
.seslp_des9wid3_bg:before,.seslp_des9wid3_bg:after{
	content:"";
	background-color:rgba(0, 0, 0, .5);
	height:100%;
	width:50%;
	left:0;
	top:0;
	position:absolute;
}
.seslp_des9wid3_bg:after{
  opacity:.7;
	left:50%;
}
.seslp_des9wid3_row{
	margin:0 -30px;
	position:relative;
}
.seslp_des9wid3_column{
	float:left;
	padding:0 30px;
	width:50%;
}
.seslp_des9wid3_column h2{
	color:#fff;
	font-size:34px;
	font-weight:500;
	letter-spacing:normal;
	padding:0;
	margin:0;
}
.seslp_des9wid3_column > p{
	font-size:14px;
	color:#e6e6e6;
}
.seslp_des9wid3_cont_list{
	margin-top:20px;
}
.seslp_des9wid3_cont_list_item{
	background-color:rgba(0, 0, 0, .7);
}
.seslp_des9wid3_cont_list_item a{
	display:block;
	padding:10px 15px;
	text-decoration:none !important;
}
.seslp_des9wid3_cont_list_item_img{
	border-radius:50%;
	float:left;
	margin-right:15px;
	height:50px;
	width:50px;
	overflow:hidden;
}
.seslp_des9wid3_cont_list_item_img img{
	object-fit:cover;
	height:100%;
	width:100%;
}
.seslp_des9wid3_cont_list_item_icon{
	float:right;
	margin-left:15px;
}
.seslp_des9wid3_cont_list_item_icon i{
	background-color:#101a24;
	border-radius:3px;
	height:40px;
	line-height:40px;
	width:40px;
	text-align:center;
	color:#fff;
	font-size:20px;
}
.seslp_des9wid3_cont_list_item_cont{
	overflow:hidden;
	display:block;
}
.seslp_des9wid3_cont_list_item_cont h3{
	background:none;
	border-width:0;
	margin:0;
	padding:0;
	letter-spacing:normal;
	font-size:18px;
	font-weight:600;
	color:#fff;
}
.seslp_des9wid3_cont_list_item_cont p{
	color:#bdbdbd;
}
.seslp_des9wid3_cont_list_item a:hover .seslp_des9wid3_cont_list_item_icon i{
	background-color:#771e44;
}
.seslp_des9wid3_cont_grid{
	margin:20px -15px 0;
}
.seslp_des9wid3_cont_grid_item{
	padding:0 15px;
	float:left;
	width:33.33%;
}
.seslp_des9wid3_cont_grid_item a{
	display:block;
	height:165px;
	overflow:hidden;
	position:relative;
}
.seslp_des9wid3_cont_grid_item_img{
	background-position:center;
	background-size:cover;
	display:block;
	height:100%;
}
.seslp_des9wid3_cont_grid_item_overlay{
	background-color:rgba(0, 112, 186, .3);
	position:absolute;
	top:0;
	right:0;
	left:0;
	bottom:0;
}
.seslp_des9wid3_cont_grid_item_overlay i{
	position:absolute;
	transform:translate(-50%,-50%);
	left:50%;
	top:50%;
	color:#fff;
	font-size:35px;
	opacity:0;
	filter:alpha(opacity=0);
}
.seslp_des9wid3_cont_grid_item a:hover .seslp_des9wid3_cont_grid_item_overlay{
	top:10px;
	left:10px;
	right:10px;
	bottom:10px;
}
.seslp_des9wid3_cont_grid_item a:hover .seslp_des9wid3_cont_grid_item_overlay i{
	opacity:1;
	filter:alpha(opacity=100);
}
.seslp_des9wid3_cont_more{
	margin-top:20px;
}
.seslp_des9wid3_column_r .seslp_des9wid3_cont_more{
	text-align:right;
}
.seslp_des9wid3_cont_more a{
	border:2px solid #fff;
	border-radius:5px;
	padding:5px 15px 3px;
	color:#fff;
	font-size:15px;
	display:inline-block;
}
.seslp_des9wid3_cont_more a:hover{
	background-color:#fff;
	color:#771e44;
	text-decoration:none;
}
/*Design9 Widget3 End*/
/*Design9 Widget4 End*/
.seslp_des9wid4_wrapper{
	padding:50px 0;
}
.seslp_des9wid4_listing{
	margin:0 -15px;
	display: flex;
  flex-wrap: wrap;
}
.seslp_des9wid4_list_item{
	margin:0 20px;
	width: calc(33.33% - 40px);
}
.seslp_des9wid4_list_item:hover .seslp_des9wid4_list_item_cont{
	transform:translateY(-10px);
	transition:all .5s ease;
}
.seslp_des9wid4_list_item_thumb{
	height:300px;
	margin-right:60px;
	position:relative;
	border-radius:4px;
}
.seslp_des9wid4_list_item_thumb a{
	display:block;
	height:100%;
}
.seslp_des9wid4_list_item_thumb_img{
	background-size:cover;
	background-position:center;
	display:block;
	height:100%;
}
.seslp_des9wid4_list_item_thumb_date{
	background-color:#771e44;
	color:#fff;
	position:absolute;
	left:20px;
	top:20px;
	text-align:center;
	width:70px;
}
.seslp_des9wid4_list_item_thumb_date span{
	display:block;
}
.seslp_des9wid4_list_item_thumb_date ._day{
	font-size:28px;
	font-weight:600;
}
.seslp_des9wid4_list_item_thumb_date ._month{
	background-color:#fff;
	color:#771e44;
	font-size:20px;
	padding:5px 0;
}
.seslp_des9wid4_list_item_cont{
	position:relative;
	background-color:#fff;
	padding:15px;
	box-shadow:2px 2px 18px 2px rgba(0, 0, 0, .05);
	margin:-130px 0 0 100px;
	min-height:250px;
	transition:all .5s ease;
}
.seslp_des9wid4_list_item_cont h3{
	background:none;
	border-width:0;
	padding:0;
	margin:0 0 5px;
	font-size:17px;
	font-weight:600;
	line-height:24px;
	text-transform:capitalize;
}
.seslp_des9wid4_list_item_cont h3 a{
	font-weight:500;
}
.seslp_des9wid4_list_item_stats{
	font-size:13px;
	margin-bottom:8px;
}
.seslp_des9wid4_list_item_stats span + span{
	margin-left:10px;
	display:inline-block;
}
.seslp_des9wid4_list_item_stats span i{
	margin-right:5px;
}
.seslp_des9wid4_list_item_des{
	margin-top:5px;
}
.seslp_des9wid4_list_item_btn{
	margin-top:10px;
}
.seslp_des9wid4_list_item_btn a{
	border:2px solid;
	display:inline-block;
	padding:8px 15px;
}
.seslp_des9wid4_list_item_btn a:hover{
	background-color:#771e44;
	color:#fff;
	text-decoration:none;
}
/*Design9 Widget4 End*/
/*Design9 Widget5 Start*/
.seslp_des9wid5_wrapper{
	padding:50px 0;
	position:relative;
}
.seslp_des9wid5_bg{
	position:absolute;
	display:block;
	background-attachment:fixed;
	background-position:center;
	background-size:cover;
	height:100%;
	width:100%;
	left:0;
	top:0;
}
.seslp_des9wid5_bg:after{
	content:"";
	background-color:rgba(0, 0, 0, .6);
	height:100%;
	width:100%;
	left:0;
	top:0;
	position:absolute;
}
.seslp_des9wid5_wrapper .seslp_des9_head h2{
	border-bottom-color:#727272;
	color:#fff;
}
.seslp_des9wid5_wrapper .seslp_blocks_container{
	position:relative;
}
.seslp_des9wid5_row{
	margin:0 -15px;
}
.seslp_des9wid5_column{
	float:left;
	padding:15px;
	width:50%;
}
.seslp_des9wid5_list_item + .seslp_des9wid5_list_item{
	margin-top:30px;
}
.seslp_des9wid5_list_item_thumb{
	float:left;
	width:190px;
	height:190px;
	overflow:hidden;
}
.seslp_des9wid5_list_item_thumb a{
	height:100%;
	display:block;
}
.seslp_des9wid5_list_item_thumb_img{
	background-size:cover;
	background-position:center;
	height:100%;
	display:block;
}
.seslp_des9wid5_list_item_cont{
	background-color:#c1c1c1;
	padding:20px;
	display:block;
	overflow:hidden;
	height:190px;
	position:relative;
}
.seslp_des9wid5_list_item_cont h3{
	background:none;
	border-width:0;
	padding:0;
	margin:5px 0;
	letter-spacing:normal;
	font-size:20px;
	font-weight:600;
	overflow:hidden;
	white-space:nowrap;
	text-overflow:ellipsis;
	line-height:25px;
}
.seslp_des9wid5_list_item_cont h3 a{
	text-decoration:none !important;
}
.seslp_des9wid5_list_item_stats span + span{
	margin-left:10px;	
}
.seslp_des9wid5_list_item_stats span i{
	margin-right:5px;
}
.seslp_des9wid5_list_item_des{	
	margin-top:5px;
}
.seslp_des9wid5_list_item_big{
	position:relative;
}
.seslp_des9wid5_list_item_big_thumb{
	height:410px;
	overflow:hidden;
}
.seslp_des9wid5_list_item_big_thumb a{
	height:100%;
	display:block;
}
.seslp_des9wid5_list_item_big_thumb_img{
	background-position:center;
	background-size:cover;
	height:100%;
	display:block;
}
.seslp_des9wid5_list_item_big_cont{
	background-color:rgba(255, 255, 255, .8);
	position:absolute;
	top:35px;
	left:35px;
	bottom:35px;
	right:35px;
	padding:20px;
	text-align:center;
}
.seslp_des9wid5_list_item_big_cont h3{
	background:none;
	border-width:0;
	padding:0;
	margin:0 0 10px;
	letter-spacing:normal;
	font-size:24px;
	font-weight:600;
	line-height:35px;
}
.seslp_des9wid5_list_item_big_cont h3 a{
	color:#000;
	text-decoration:none !important;
}
.seslp_des9wid5_list_item_big_stats{
	margin-bottom:10px;
}
.seslp_des9wid5_list_item_big_stats, .seslp_des9wid5_list_item_big_stats a{
	color:#3f3f3f !important;
	font-size:14px;
}
.seslp_des9wid5_list_item_big_stats span + span{
	margin-left:10px;	
}
.seslp_des9wid5_list_item_big_stats span i{
	margin-right:5px;
}
.seslp_des9wid5_list_item_big_des{
	font-size:15px;
	color:#696969;
}
.seslp_des9wid5_list_item:hover .seslp_des9wid5_list_item_thumb_img,
.seslp_des9wid5_list_item_big:hover .seslp_des9wid5_list_item_big_thumb_img{
	transform:scale(1.05);
}
/*Design9 Widget5 End*/
@media (max-width:1200px){
	/*Widget 3*/
	.seslp_des9wid2_wrapper .seslp_blocks_container{border-width:0;}
	.seslp_des9wid2_wrapper .seslp_des9_head{padding:0;}
}
@media (max-width:767px){
	.seslp_des9wid2_wrapper, .seslp_des9wid3_wrapper, .seslp_des9wid4_wrapper, .seslp_des9wid5_wrapper{padding:20px 0;}
	.seslp_des9_head h2{font-size:25px;padding:10px 0;margin-bottom:20px;}
	/*Widget 2*/
	.seslp_des9wid2_feature_item{display:block;padding:30px 20px;width:100%;}
	.seslp_des9wid2_feature_item + div{border-width:1px 0 0;}
	/*Widget 3*/
	.seslp_des9wid3_wrapper, .seslp_des9wid3_wrapper .seslp_blocks_container{padding:0;}
	.seslp_des9wid3_bg:before, .seslp_des9wid3_bg:after{display:none;}
	.seslp_des9wid3_row{margin:0;}
	.seslp_des9wid3_column h2{font-size:25px;}
	.seslp_des9wid3_column{padding:10px; width:100%;}
	.seslp_des9wid3_column_l{background-color:rgba(0, 0, 0, .4);}
	.seslp_des9wid3_column_r{background-color:rgba(119, 30, 68, .6);}
	.seslp_des9wid3_cont_more{text-align:right;margin-bottom:10px;}
	.seslp_des9wid3_cont_grid{margin-left:-5px;margin-right:-5px;}
	.seslp_des9wid3_cont_grid_item{padding:0 5px;}
	/*Widget 5*/
	.seslp_des9wid4_listing{margin:0;overflow:hidden;}
	.seslp_des9wid4_list_item{width:100%;padding:5px 0;}
	/*Widget 6*/
	.seslp_des9wid5_row{margin:0;}
	.seslp_des9wid5_column{width:100%;padding:5px 0;}
	.seslp_des9wid5_list_item + .seslp_des9wid5_list_item{margin-top:10px;}
	.seslp_des9wid5_list_item_big_thumb{height:300px;}
	.seslp_des9wid5_list_item_big_cont{left:15px;top:15px;bottom:15px;right:15px;}
	.seslp_des9wid5_list_item_big_cont h3{font-size:25px;line-height:30px;margin:0;}
	.seslp_des9wid5_list_item_big_stats{margin:0;}
}
@media (max-width:600px){
	/*Widget 5*/
	.seslp_des9wid4_list_item{}
	.seslp_des9wid4_list_item article{overflow:hidden;border-width:1px;}
	.seslp_des9wid4_list_item_thumb{float:left;margin:0;width:200px;height:200px;}
	.seslp_des9wid4_list_item_cont{overflow:hidden;display:block;margin:0;box-shadow:none;min-height:inherit;border-width:0;}
	.seslp_des9wid4_list_item_des{display:none;}
	.seslp_des9wid4_list_item_cont h3{font-size:17px;}
}
@media (max-width:480px){
	/*Widget 4*/
	.seslp_des9wid4_list_item_thumb{
		width:100%;
	}
	.seslp_des9wid3_cont_grid_item{width:100%;padding:5px;}
	/*Widget 5*/
	.seslp_des9wid5_list_item_thumb, .seslp_des9wid5_list_item_cont{width:100%;text-align:center;}
	.seslp_des9wid5_list_item_big_thumb{height:190px;}	
	.seslp_des9wid5_list_item_big_cont{left:0;top:0;bottom:0;right:0;position:inherit;padding:15px;}
	.seslp_des9wid5_list_item_stats, .seslp_des9wid5_list_item_big_stats{font-size:14px;}
	.seslp_des9wid5_list_item_cont h3, .seslp_des9wid5_list_item_big_cont h3{font-size:17px;line-height:25px;}
	
}
/*DESIGN 9 ENDS HERE*/