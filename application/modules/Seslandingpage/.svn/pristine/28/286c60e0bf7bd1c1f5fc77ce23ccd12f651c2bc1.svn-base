/*DESIGN 10 Start HERE*/
.seslp_des10_head{
	position:relative;
	margin-bottom:10px;
	overflow:hidden;
}
.seslp_des10_head h2{
	float:left;
	padding:0 30px 0 0;
	margin:0;
	font-size:32px;
	letter-spacing:normal;
	font-weight:500;
}
.seslp_des10_head h2:after{
	content:"";
	border-top:1px solid #e4e4e4;
	position:absolute;
	top:50%;
	margin-left:30px;
	width:100%;
}
/*Design9 Widget2 Start*/
.seslp_des10wid2_wrapper{
	padding:50px 0;
	position:relative;
}
.seslp_des10wid2_des{
	max-width:760px;
	font-size:16px;
}
.seslp_des10wid2_listing{
	margin-top:30px;
}
.seslp_des10wid2_list_item{
	float:left;
	padding-right:20px;
}
.seslp_des10wid2_list_item_thumb{
	height:170px;
	position:relative;
	overflow:hidden;
	width:170px;
}
.seslp_des10wid2_list_item_thumb a{
	display:block;
	height:100%;
}
.seslp_des10wid2_list_item_thumb_img{
	background-position:center;
	background-size:cover;
	display:block;
	height:100%;
}
.seslp_des10wid2_list_item_thumb_overlay{
	background-color:#000;
	opacity:.6;
	filter:alpha(opacity=60);
	position:absolute;
	height:100%;
	width:100%;
	top:0;
}
.seslp_des10wid2_list_item_thumb_icon{
	border:2px solid #fff;
	border-radius:50%;
	height:50px;
	width:50px;
	position:absolute;
	left:50%;
	top:50%;
	transform:translate(-50%,-50%);
	line-height:50px;
	text-align:center;
}
.seslp_des10wid2_list_item_thumb_icon i{
	color:#fff;
	font-size:17px;
}
.seslp_des10wid2_list_item_thumb:hover .seslp_des10wid2_list_item_thumb_img{
	transform:scale(1.05);
}
/*Design10 Widget2 End*/
/*Design10 Widget3 Start*/
.seslp_des10wid3_wrapper{
	padding:50px 0;
	position:relative;
}
.seslp_des10wid3_wrapper .seslp_blocks_container{
	position:relative;
}
.seslp_des10wid3_bg{
	position:absolute;
	display:block;
	background-attachment:fixed;
	background-position:center;
	background-size:cover;
	height:100%;
	width:100%;
	left:0;
	top:0;
}
.seslp_des10wid3_bg:after{
	content:"";
	background-color:rgba(0, 0, 0, .8);
	height:100%;
	width:100%;
	left:0;
	top:0;
	position:absolute;
}
.seslp_des10wid3_row{
	margin:0 -15px;
}
.seslp_des10wid3_column{
	float:left;
	padding:0 15px;
	width:50%;
}
.seslp_des10wid3_column_left_item{
	height:530px;
	overflow:hidden;
}
.seslp_des10wid3_column_left_item a{
	display:block;
	position:relative;
	height:100%;
}
.seslp_des10wid3_column_left_item_img{
	background-size:cover;
	background-position:center;
	display:block;
	height:100%;
}
.seslp_des10wid3_column_left_item_overlay{
	background-color:#000;
	opacity:.1;
	filter:alpha(opacity=10);
	position:absolute;
	height:100%;
	width:100%;
	top:0;
}
.seslp_des10wid3_column_right .seslp_des10_head h2{
	color:#fff;
}
.seslp_des10wid3_des{
	font-size:16px;
	color:#c4c4c4;
}
.seslp_des10wid3_listing{
	margin:10px -15px 0;
}
.seslp_des10wid3_list_item{
	float:left;
	padding:15px;
	width:33.33%;
}
.seslp_des10wid3_list_item_thumb{
	height:170px;
	position:relative;
	overflow:hidden;
}
.seslp_des10wid3_list_item_thumb a{
	display:block;
	height:100%;
}
.seslp_des10wid3_list_item_thumb_img{
	background-size:cover;
	background-position:center;
	display:block;
	display:block;
	height:100%;
}
.seslp_des10wid3_list_item_thumb_overlay{
	background-color:#000;
	opacity:.6;
	filter:alpha(opacity=60);
	position:absolute;
	height:100%;
	width:100%;
	top:0;
}
.seslp_des10wid3_list_item_thumb_btn{
	position:absolute;
	left:50%;
	top:50%;
	transform:translate(-50%,-50%);
}
.seslp_des10wid3_list_item_thumb_btn i{
	font-size: 30px;
  color: #b5b5b5;
}
.seslp_des10wid3_column_left_item:hover .seslp_des10wid3_column_left_item_img{
	transform:scale(1.05);
}
.seslp_des10wid3_list_item_thumb:hover .seslp_des10wid3_list_item_thumb_img{
	transform:scale(1.05);
}
.seslp_des10wid3_list_item_thumb:hover .seslp_des10wid3_list_item_thumb_btn i{
	color:#dc0000;
}
/*Design10 Widget3 End*/
/*Design10 Widget4 Start*/
.seslp_des10wid4_wrapper{
	padding:50px 0;
}
.seslp_des10wid4_des{
	max-width:760px;
	font-size:14px;
}
.seslp_des10wid4_row{
	margin:30px -15px 0;
}
.seslp_des10wid4_column{
	float:left;
	padding:0 15px;
	width:50%;
}
.seslp_des10wid4_list_item{
	background-color:#f1f1f1;
	position:relative;
	margin-bottom:25px;
}
.seslp_des10wid4_list_item:nth-last-child(1){
	margin-bottom:0;
}
.seslp_des10wid4_list_item_thumb{
	float:left;
	height:150px;
	width:190px;
	overflow:hidden;
}
.seslp_des10wid4_list_item_thumb > a{
	display:block;
	height:100%;
}
.seslp_des10wid4_list_item_thumb_img{
	background-size:cover;
	background-position:center;
	display:block;
	height:100%;
}
.seslp_des10wid4_list_item_cont{
	background-color:#fff;
	position:absolute;
	top:20px;
	right:30px;
	bottom:20px;
	left:150px;
	padding:20px;
}
.seslp_des10wid4_list_item_stats{
	white-space:nowrap;
	text-overflow:ellipsis;
	overflow:hidden;
	padding-left:1px;
}
.seslp_des10wid4_list_item_stats > span + span{
	margin-left:10px;
}
.seslp_des10wid4_list_item_stats i{
	margin-right:5px;
}
.seslp_des10wid4_list_item_cont h3{
	background:none !important;
	border-width:0 !important;
	font-size:20px;
	padding:0;
	margin:10px 0 0;
	line-height:25px;
}
.seslp_des10wid4_list_item_cont h3 a{
	font-weight:500;
}
.seslp_des10wid4_list_item:hover{
	background-color:#dc0000;
}
.seslp_des10wid4_list_item:hover .seslp_des10wid4_list_item_thumb_img{
	transform:scale(1.05);
}
.seslp_des10wid4_sigl_item{
	position:relative;
	height:500px;
}
.seslp_des10wid4_sigl_item_thumb{
	height:100%;
	position:relative;
	overflow:hidden;
}
.seslp_des10wid4_sigl_item_thumb > a{
	display:block;
	height:100%;
}
.seslp_des10wid4_sigl_item_thumb a span{
	background-position:center;
	background-size:cover;
	display:block;
	height:100%;
}
.seslp_des10wid4_sigl_item_thumb > a:after{
	content:"";
	height:100%;
	width:100%;
	position:absolute;
	left:0;
	top:0;
	background-color:#000;
	opacity:.6;
	filter:alpha(opacity=60);
}
.seslp_des10wid4_sigl_item_cont{
	position:absolute;
	bottom:0;
	padding:30px;
	width:100%;
}
.seslp_des10wid4_sigl_item_date{
	font-size:17px;
	color:#fff;
}
.seslp_des10wid4_sigl_item_date a{
	color:white;
}
.seslp_des10wid4_sigl_item_cont h3{
	background:none !important;
	border-width:0 !important;
	font-size:30px;
	font-weight:600;
	line-height:40px;
	padding:0;
	margin:5px 0;
}
.seslp_des10wid4_sigl_item_cont h3 a{
	color:#fff !important;
	font-weight:500; 
}
.seslp_des10wid4_sigl_item_stats{
	color:#c0bfbf;
	font-size:15px;
}
.seslp_des10wid4_sigl_item_stats i{
	margin-right:5px;
}
.seslp_des10wid4_sigl_item_des{
	border-top:1px solid #ddd;
	padding-top:10px;
	color:#fff;
	font-size:15px;
	margin-top:10px;
}
.seslp_des10wid4_sigl_item:hover .seslp_des10wid4_sigl_item_thumb a span{
	transform:scale(1.05);
}
/*Design10 Widget4 End*/
/*Design10 Widget5 Start*/
.seslp_des10wid5_wrapper{
	background-color:#f0f0f0;
	padding:50px 0;
}
.seslp_des10wid5_des{
	font-size:16px;
	max-width:760px;
}
.seslp_des10wid5_row{
	margin:30px -15px 0;
}
.seslp_des10wid5_list_item{
	float:left;
	padding:0 15px;
	width:16.66%;
}
.seslp_des10wid5_list_item_thumb{
	margin:0 auto;
	width:160px;
	height:160px;
	overflow:hidden;
	border-radius:50%;
}
.seslp_des10wid5_list_item_thumb a{
	display:block;
	height:100%;
}
.seslp_des10wid5_list_item_thumb a span{
	background-position:center;
	background-size:cover;
	display:block;
	height:100%;
}
.seslp_des10wid5_list_item_name{
	text-align:center;
	font-size:17px;
	margin-top:10px;
}
.seslp_des10wid5_list_item:hover .seslp_des10wid5_list_item_thumb a span{
	transform:scale(1.05);
} 
/*Design10 Widget5 End*/
/*Design10 Widget6 Start*/
.seslp_des10wid6_wrapper{
	padding:50px 0;
}
.seslp_des10wid6_des{
	max-width:760px;
	font-size:14px;
}
.seslp_des10wid6_row{
	margin:30px -15px 0;
	display:flex;
	flex-wrap:wrap;
}
.seslp_des10wid6_list_item{
	margin:0 10px;
	width:calc(33.33% - 30px);
}
.seslp_des10wid6_list_item article{
	border-radius:5px;
	overflow:hidden;
	box-shadow:0px 3px 6px rgba(0, 0, 0, .2);
}
.seslp_des10wid6_list_item_thumb{
	overflow:hidden;
	border-radius:5px 5px 0 0;
	height:210px;
}
.seslp_des10wid6_list_item_thumb a{
	display:block;
	height:100%;
}
.seslp_des10wid6_list_item_thumb a span{
	background-size:cover;
	background-position:center;
	display:block;
	height:100%;
}
.seslp_des10wid6_list_item_owner_photo{
	border-radius:50%;
	border:5px solid #fff;
	height:80px;
	width:80px;
	margin:-40px auto 10px;
	overflow:hidden;
	position:relative;
}
.seslp_des10wid6_list_item_owner_photo img{
	height:100%;
	width:100%;
	object-fit:cover;
}
.seslp_des10wid6_list_item_info{
	text-align:center;
	padding:0 20px;
	min-height:200px;
}
.seslp_des10wid6_list_item_info h3{
	background:none !important;
	border-width:0 !important;
	padding:0;
	margin:0 0 5px;
	font-size:20px;
	font-weight:600;
	line-height:25px;
}
.seslp_des10wid6_list_item_stats > span + span{
	margin-left:10px;
}
.seslp_des10wid6_list_item_stats i{
	margin-right:5px;
}
.seslp_des10wid6_list_item_des{
	margin-top:16px;
	margin-top:20px;
}
.seslp_des10wid6_list_item article:hover .seslp_des10wid6_list_item_thumb a span{
	transform:scale(1.05);
}
/*Design10 Widget6 End*/
@media (max-width: 767px){
	.seslp_des10wid2_wrapper, .seslp_des10wid3_wrapper, .seslp_des10wid4_wrapper, .seslp_des10wid5_wrapper, .seslp_des10wid6_wrapper{
		padding:20px 0;
	}
	.seslp_des10_head{margin-bottom:0;}
	.seslp_des10_head h2{font-size:25px;}
	.seslp_des10wid2_listing, .seslp_des10wid4_row, .seslp_des10wid5_row, .seslp_des10wid6_row{
		margin-top:10px;
	}
	/*Widget 2*/
	.seslp_des10wid2_list_item{width:25%;padding:10px;}
	.seslp_des10wid2_list_item_thumb{width:100%;height:150px;}
	
	/*Widget 3*/
	.seslp_des10wid3_column_left{display:none;}
	.seslp_des10wid3_column{width:100%;}
	.seslp_des10wid3_column_left_item{height:200px;}
	/*Widget 4*/
	.seslp_des10wid4_column{width:100%;margin:5px 0;}
	.seslp_des10wid4_list_item{margin-bottom:10px;}
	.seslp_des10wid4_sigl_item{height:350px;}
	/*Widget 5*/
	.seslp_des10wid5_list_item{width:33.33%;margin-bottom:10px;}
	/*Widget 6*/
	.seslp_des10wid6_list_item{float:none;width: 100%;max-width: 370px;margin: 0 auto 15px;}
}
@media (max-width: 600px){
	/*Widget 2*/
	.seslp_des10wid2_listing{margin-left:-5px;margin-right:-5px;}
	.seslp_des10wid2_list_item{width:50%;padding:5px;}
	/*Widget 3*/
	.seslp_des10wid3_row{margin:0 -5px;}
	.seslp_des10wid3_list_item{width:50%;padding:5px;}
	/*Widget 4*/
	.seslp_des10wid4_list_item_cont{left:50px;}
	/*Widget 5*/
	.seslp_des10wid5_list_item{width:50%;}
}
@media (max-width: 480px){
	/*Widget 4*/
	.seslp_des10wid4_list_item_thumb{width:100%;}
	.seslp_des10wid4_list_item_cont{left:20px;right:20px;}
	.seslp_des10wid4_sigl_item{height:250px;}
	.seslp_des10wid4_sigl_item_cont{padding:15px;}
	.seslp_des10wid4_sigl_item_cont h3{font-size:20px;line-height:25px;}
	.seslp_des10wid4_sigl_item_des{display:none;}
	/*Width 5*/
	.seslp_des10wid5_list_item_thumb{height:130px;width:130px;}
}
/*DESIGN 10 ENDS HERE*/