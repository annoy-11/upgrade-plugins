/*DESIGN 1 START HERE*/
/*Design1 Widget2 Starts*/
.seslp_des1wid2_wrapper{
	padding:0 0 60px;
	position:relative;
}
.seslp_des1wid2_wrapper:after{
	content: "";
	width: 100%;
	height: 5px;
	left: 0;
	right: 0;
	position: absolute;
	background-size: 10px 5px;
	top: auto;
	bottom: -5px;
	z-index: 1;
	transform: rotate(180deg);
}
.seslp_des1wid2_features {
	display: flex;
	flex-wrap: wrap;
}
.seslp_des1wid2_wrapper h2{
	border-bottom:1px dashed #d1d1d1;
	font-size:34px;
	letter-spacing:normal;
	text-align:center;
	position:relative;
	padding:15px 0;
	margin:0 0 20px;
}
.seslp_des1wid2_wrapper h2:after{
	border-bottom:1px dashed #ffbb00;
	bottom:-1px;
	content:"";
	left:50%;
	margin-left:-150px;
	position:absolute;
	width:300px;
}
.seslp_des1wid2_des{
	color:#999;
	text-align:center;
	margin:0 0 20px;
	font-size:15px;
}
.seslp_des1wid2_cont{
	position:relative;
}
.seslp_des1wid2_img{
	border-radius:50%;
	border:2px dashed #ffbb00;
	width:400px;
	height:400px;
	margin:0 auto;
	padding:80px;
}
.seslp_des1wid2_img span{
	background-position:center center;
	background-size:cover;
	border-radius:50%;
	display:block;
	height:100%;
	width:100%;
}
.seslp_des1wid2_feature_item{
	width: calc(33% - 30px);
	margin: 0 15px;
	position:relative;
}
.seslp_des1wid2_feature_item:last-child:before {
  display: none;
}
.seslp_des1wid2_feature_item:before {
	content: "";
	position: absolute;
	width: 100px;
	right: -20%;
	top: 30%;
	border: 1px dashed;
}
.seslp_des1wid2_feature_item_icon{
	border-radius: 50%;
	height: 100px;
	position: relative;
	width: 100px;
	margin: auto;
}
.seslp_des1wid2_feature_item_icon i{
	text-align: center;
	background-repeat: no-repeat;
	background-position: center center;
	height: 100%;
	width: 50px;
	position: absolute;
	left: 0;
	background-size: 100%;
	right: 0;
	margin: auto;
}
.seslp_des1wid2_feature_item_icon i:before{
	color:#ffbb00;
	font-size:35px;
	line-height:100px;
}
.seslp_des1wid2_feature_item_cont{
	overflow:hidden;
	display:block;
	text-align:center;
}
.seslp_des1wid2_feature_item_cont h3{
	font-size:20px;
	line-height:30px;
	margin:15px 0 0;
	font-weight:600;
}
.seslp_des1wid2_feature_item_cont p{
	font-size:14px;
}
/*Design1 Widget2 End*/
/*Design1 Widget3 Starts*/
.seslp_des1wid3_wrapper{
	padding:50px 0 70px;
	position:relative;
}
.seslp_des1wid3_background,
.seslp_des1wid3_background:after{
	background-position:center;
	background-attachment:fixed;
	position:absolute;
	height:100%;
	width:100%;
	left:0;
	top:0;
	background-size:100%;
}
.seslp_des1wid3_background:after {
	content: "";
	background: black;
	opacity: .6;
}
.seslp_des1wid3_wrapper .seslp_blocks_container{
	position:relative;
	z-index:1;
}
.seslp_des1wid3_wrapper h2{
	border-bottom:1px dashed #d1d1d1;
	color:#fff;
	font-size:34px;
	letter-spacing:normal;
	text-align:center;
	position:relative;
	padding:15px 0;
	margin:0 0 20px;
}
.seslp_des1wid3_wrapper h2:after{
	border-bottom:1px dashed #ffbb00;
	bottom:-1px;
	content:"";
	left:50%;
	margin-left:-150px;
	position:absolute;
	width:300px;
}
.seslp_des1wid3_des{
	color:#999;
	text-align:center;
	margin:0 0 20px;
	font-size:15px;
}
.seslp_des1wid3_listings{
	float:left;
	margin-top:40px;
	width:100%;
}
.seslp_des1wid3_listing_item{
	float:left;
	padding:0 15px;
	width:25%;
}
.seslp_des1wid3_listing_item article{
	max-width:260px;
}
.seslp_des1wid3_listing_item_thumb{
	box-shadow:8px 8px 12px 0 rgba(0,0,0,.12), -8px -8px 12px 0 rgba(0,0,0,.12);
	background-color:rgba(107, 107, 107, .3);
	border-radius:50%;
	position:relative;
	display:block;
	padding:15px;
	height:260px;
	width:260px;
}
.seslp_des1wid3_listing_item_thumb:hover{
	background-color:#ffbb00;
}
.seslp_des1wid3_listing_item_thumb_img,
.seslp_des1wid3_listing_item_thumb_img span{
	border-radius:50%;
	background-position:center;
	background-size:cover;
	display:block;
	overflow:hidden;
	height:100%;
	width:100%;
}
.seslp_des1wid3_listing_item_thumb:hover .seslp_des1wid3_listing_item_thumb_img span{
	transform:scale(1.1);
}
.seslp_des1wid3_listing_item_stats{
	left:0;
	position:absolute;
	top:0;
	width:100%;
}
.seslp_des1wid3_listing_item_thumb:hover .seslp_des1wid3_listing_item_stats{
	opacity:0;
	top:-20px;
}
.seslp_des1wid3_listing_item_stats p{
	border-radius:50%;
	font-size:12px;
	color:#fff;
	display:block;
	height:40px;
	line-height:40px;
	position:absolute;
	text-align:center;
	width:40px;
	z-index:2;
	opacity:.9;
}
.seslp_des1wid3_listing_item_stats p i{
	margin-right:2px;
}
.seslp_des1wid3_listing_item_stats p._likes{
	left:50%;
	margin-left:-20px;
	margin-top:-10px;
}
.seslp_des1wid3_listing_item_stats p._comments{
	left:40px;
	margin-top:10px;
}
.seslp_des1wid3_listing_item_stats p._views{
	right:40px;
	margin-top:10px;
}
.seslp_des1wid3_listing_item_stats p._fav{
	left:0;
	margin-top:60px;
}
.seslp_des1wid3_listing_item_stats p._rating{
	right:0;
	margin-top:60px;
}

.seslp_des1wid3_listing_item_title{
	background-color:rgba(0, 0, 0, 0.5);
	position:absolute;
	z-index:1;
	top:15px;
	left:15px;
	bottom:15px;
	right:15px;
	display:block;
	text-align:center;
	padding:0 15px 8px 15px;
	overflow:visible;
	border-radius:50%;
	font-size:0;
}
.seslp_des1wid3_listing_item_title:before {
	content:'';
	display:inline-block;
	height:100%;
	vertical-align:middle;
}
.seslp_des1wid3_listing_item_title span{
	width:100%;
	display:inline-block;
	vertical-align:middle;
	font-size:14px;
	font-weight:500;
	letter-spacing:1px;
	color:#fff;
	white-space:pre-line;
	padding-left:20px;
	padding-right:20px;
}
.seslp_des1wid3_listing_item_info{
	color:#fff;
	font-size:13px;
	padding-top:20px;
	text-align:center;
}
.seslp_des1wid3_listing_item_info p._location{
	border-bottom:1px dashed #444;
	padding-bottom:5px;
	margin-bottom:5px;
}
.seslp_des1wid3_listing_item_info p._location span{
	display:block;
	text-overflow:ellipsis;
	white-space:nowrap;
	overflow:hidden;
}
.seslp_des1wid3_listing_item_info p._location span i{
	font-size:17px;
}
.seslp_des1wid3_btm{
	padding:50px 0px;
	width:100%;
}
.seslp_des1wid3_btm_item{
	float:left;
	padding:0 15px;
	width:33.33%;
}
.seslp_des1wid3_btm_item_thumb{
	border-radius:5px;
	height:220px;
	overflow:hidden;
	position:relative;
	width:100%;
}
.seslp_des1wid3_btm_item_thumb > a{
	display:block;
	height:100%;
	width:100%;
} 
.seslp_des1wid3_btm_item_thumb_img{
	background-size:cover;
	background-position:center;
	display:block;
	height:100%;
	width:100%;
}
.seslp_des1wid3_btm_item_thumb:hover .seslp_des1wid3_btm_item_thumb_img{
	transform:scale(1.1);
}
.seslp_des1wid3_btm_item_cont{
	background-color:rgba(0, 0, 0, .5);
	position:absolute;
	left:0;
	bottom:0;
	padding:30px;
	top:0;
	right:0;
}
.seslp_des1wid3_btm_item_cont > div{
	border:1px dashed #fff;
	display:table;
	height:100%;
	position:relative;
	width:100%;
}
.seslp_des1wid3_btm_item_cont > div p{
	color:#fff;
	display:table-cell;
	font-size:24px;
	font-weight:500;
	line-height:36px;
	text-align:center;
	vertical-align:middle;
}
.seslp_des1wid3_btm_item_cont > div:before,
.seslp_des1wid3_btm_item_cont > div:after,
.seslp_des1wid3_btm_item_cont > div > p:before,
.seslp_des1wid3_btm_item_cont > div > p:after{
	content:"";
	position:absolute;
	height:9px;
	width:16px;
}
.seslp_des1wid3_btm_item_cont > div:before{
	left:-16px;
	top:-9px;
	border:1px dashed #fff;
	border-width:0 1px 1px 0;
}
.seslp_des1wid3_btm_item_cont > div:after{
	right:-16px;
	top:-9px;
	border:1px dashed #fff;
	border-width:0 0 1px 1px;
}
.seslp_des1wid3_btm_item_cont > div > p:before{
	left:-16px;
	bottom:-9px;
	border:1px dashed #fff;
	border-width:1px 1px 0 0;
}
.seslp_des1wid3_btm_item_cont > div > p:after{
	right:-16px;
	bottom:-9px;
	border:1px dashed #fff;
	border-width:1px 0 0 1px;
}
/*Design1 Widget3 Starts*/

