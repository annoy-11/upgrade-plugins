
.estore_icon_add{background-image:url(~/application/modules/Estore/externals/images/add.png);}
.estore_icon_back{background-image:url(~/application/modules/Estore/externals/images/back.png);}
.estore_icon_import{
	background-image:url(~/application/modules/Estore/externals/images/import.png);
	background-size: contain;
}
.sesbasic_nav_btns._estore {
  margin: 0 0 10px 0;
}
.estore_import_buttons a {
  border: 2px solid #0DC7F1;
  padding: 10px;
  display: inline-block;
  width: 230px;
  text-decoration: none !important;
  font-size: 15px;
  line-height: 16px;
  margin: 0 3px;
  text-align: center;
}
.estore_search_reasult{
	font-weight:bold;
	margin-bottom:10px;
}
/*Show Details*/
.estore_view_stats_popup{
	padding: 20px;
	width:600px;
}
.estore_view_popup_con .estore_popup_img_store{
	overflow: hidden;
	display: block;
	margin-bottom:10px;
	border-bottom-width: 1px;
	padding-bottom: 10px;
}
.estore_view_popup_con .estore_popup_img_store p.popup_img img{
	margin-right: 15px;
	float: left;
	width: 130px;
}
.estore_view_popup_con .estore_popup_img_store p.popup_title{
	font-size: 15px;
	font-weight: bold;
	margin-bottom:8px;
}
.estore_view_popup_con .estore_popup_img_store p.owner_title{
	margin-bottom:8px;
}
.estore_view_popup_con .estore_popup_img_store p.owner_title b{
	font-weight:bold;
	min-width:100px;
	display:inline-block;
}
.estore_view_popup_con .estore_popup_owner_store p.owner_title{
	margin-bottom:8px;;
}
.estore_view_popup_con .estore_popup_owner_store p.owner_title b{
	font-weight:bold;
}
.estore_view_popup_con .estore_popup_owner_store p.owner_des{
	text-align:justify;
}
/*Clain Request Status Form*/
.estore_claim_approve_form{
	padding:15px;
}
.estore_claim_approve_form #approve_decline-wrapper{
	margin:10px 0;
}
.estore_claim_approve_form #approve_decline-label{
	margin-bottom:5px;
}

/*Popup Form*/
.estore_admin_popup_form{
	margin:15px 0 0 15px;
	width:600px;
}
.estore_admin_popup_form div.form-element input[type="text"],
.estore_admin_popup_form div.form-element textarea,
.estore_admin_popup_form select{
	width:100% !important;
	max-width:100%;
}
.settings.global_form_popup .form-label{
	width: 160px;
}
.settings.global_form_popup input[type="text"],
.settings.global_form_popup textarea,
.settings.global_form_popup select{
	width: 100%;
}
.importlisting_form h3{
	margin-bottom: 10px;
}

/*Review Add Parameter Popup*/
.estore_parameter_popup{
  width:450px;
}
.estore_parameter_popup .settings .form-wrapper{
  padding: 0;
}
.estore_parameter_popup .form-label{
  display:none;
}
.estore_parameter_popup input[type=text]{
  margin:0 10px 10px 0;
}
.estore_parameter_popup .estore_added_parameter{
  margin-right: 10px;
}
.estore_parameter_popup .fa:before{
  margin-right:5px;
}
.estore_parameter_popup a.fa:hover{
  text-decoration:none;
}

/*Store Dashboard Css */
.estore_dashboard_store_info{
	background: $theme_list_background_color
}

/*Manage Table*/
.estore_manage_table{
	width:100%;
}
.estore_manage_table *{
  -webkit-box-sizing:border-box;
  -moz-box-sizing:border-box;
  box-sizing:border-box;
}
.estore_manage_table_head{
	background-color:#f5f5f5;
	border-bottom:1px solid #aaa;
	display:block;
	overflow:hidden;
}
.estore_manage_table_head > div{
	padding:10px;
	float:left;
	font-weight:bold;
	padding-top:7px;
	padding-bottom:7px;
	white-space:nowrap;
}
.estore_manage_table_list{
	margin-bottom:10px;
	width:100% !important;
}
.estore_manage_table_list li{
	border-bottom:1px solid #eee;
	cursor:move;
	clear:both;
	overflow:hidden;
}
.estore_manage_table_list li:nth-child(odd){
	background-color:#fff;
}
.estore_manage_table_list li:nth-child(even) {
	background-color:#f8f8f8;
}
.estore_manage_table_list li > div{
	padding:10px;
	font-size:.9em;
	float:left;
	padding-top:7px;
	padding-bottom:7px;
	vertical-align:top;
	white-space:normal;
}
/*Search Form*/
.estore_admin_search_form{
  -moz-border-radius: 5px;
  -webkit-border-radius: 5px;
  border-radius: 5px;
  background-color: #f5f5f5;
}
.estore_admin_search_form > form{
	overflow: hidden;
	border: 1px solid #ddd;
	background: #fff;
	padding:5px;
}
.estore_admin_search_form form > div{
	margin-bottom: 10px;
}
.estore_admin_search_form #commission_min-element{
	display: inline-block;
}
.estore_admin_search_form form .form-elements > div{
	margin:5px;
}
.estore_admin_search_form #integer-wrapper .form-wrapper .form-label{
	display:none;
}
.estore_admin_search_form #integer-wrapper .form-wrapper{
	margin-top:3px;
	display: inline-block;
	float: left;
}
.estore_admin_search_form #integer-wrapper .form-wrapper input{
	width:60px;
	float: left;
	margin:0 4px;
	display: inline-block;
}
.estore_admin_search_form form .form-label label{
	margin-bottom:5px;
	font-size: 8pt;
	font-weight: bold;
	display: block;
}
.estore_admin_search_form form input[type="text"],
.estore_admin_search_form form select{
	line-height: 30px;
	padding: 0 10px;
	display: block;
	height: 32px;
}
.add_country_admin select{
	width:100%;
}
.add_country_admin #addStatesBox{
	margin-top:10px;    
	display: inline-block;
}
.add_country_admin .remove-elem {
	position: absolute;
	margin-left: -10px;
	margin-top: -5px;
}
.add_country_admin .input-estore-elem {
  margin-bottom: 15px;
}