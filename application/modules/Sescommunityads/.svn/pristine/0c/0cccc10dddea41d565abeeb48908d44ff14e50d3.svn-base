
/*Notifications*/
.notification_type_sescommunityads_newadscreateadmin:before,
.notification_type_sescommunityads_adminapproval:before{
	content:"\\f0a1";
}
.notification_type_sescommunityads_adsapprove:before{
	content:"\\f00c";
}
/*Generic Layout Container*/
[class*='layout_sescommunityads_'], [id^='global_page_sescommunityads-'] .layout_core_content{clear:both;margin-bottom:15px;}
[class*='layout_sescommunityads_']:after, [id^='global_page_sescommunityads-'] .layout_core_content:after{clear:both;content:'';display:block;}
[class*='tab_layout_sescommunityads_']{margin:0;clear:none;}
.sescmads_payment_btn{
	background-color:$theme_button_background_color;
	color:$theme_button_font_color !important;
	display:block;
	text-align:center;
	line-height:30px;
	border-radius:3px;
	font-weight:bold;
}
.sescmads_payment_btn:hover{
	opacity:.9;
}
.sescmads_payment_btn i{
	margin-right:5px;
}
/*Packages*/
.sescmads_packages_main{
	background-color:$theme_list_background_color_hover;
}
.sescmads_packages_table_item > section,
.sescmads_packages_list_item > section{
	background-color:$theme_list_background_color;
}
.sescmads_packages_create_btn,
.sescmads_packages_list_item ._top ._title h5{
	background-color:$theme_button_background_color;
	color:$theme_button_font_color !important;
}
.sescmads_packages_create_btn:hover{
	background-color:$theme_button_background_hover_color;
}
.sescmads_packages_table_item.active > section,
.sescontest_packages_list_item.active > section{
  border:3px solid $theme_font_color;
}
/*Choose Ad*/
/*.sescmads_select_ads_container{
	background-color:$theme_list_background_color_hover;
}*/
/*Select Ad Type*/
.sescmads_create_step_header,
.sescmads_create_step_header button{
	color:$theme_font_color !important;
}
.sescmads_select_ads ul li:hover{
	background:$theme_button_background_color;
}
.sescmads_select_ads ul li:hover *{
	color:$theme_button_font_color;
}
/*Create Ads*/
.sescmads_create_container{
	background-color:$theme_list_background_color_hover;
}
.sescmads_create_step{
	background-color:$theme_list_background_color;
}
.sescmads_create_step_footer a._btnh{
	background-color:$theme_button_background_color;
	color:$theme_button_font_color;
}
.sescmads_choose_format > ul > li.active article{
	border-color:$theme_button_background_color;
}
.sescmads_choose_format > ul > li.active article ._tick{
	background-color:$theme_button_background_color;
	border-color:$theme_button_background_color;
	color:$theme_button_font_color;
}
.sescmads_create_carousel .owl-nav > div{
	background-color:$theme_button_background_color;
	color:$theme_button_font_color;
}
.sescmads_create_cont_nav li a,
.sescmads_create_carousel_item{
	background-color:$theme_list_background_color_hover;
}
.sescmads_create_cont_nav li.sescomm_active a{
	border:1px solid $theme_button_background_color;
}
.sescmads_create_cont_nav li.sescomm_active:after{
	border-bottom:8px solid $theme_list_background_color_hover;
}
.sescmads_create_targetting_tabs{
	background-color:$theme_list_background_color_hover;
}
.sescmads_create_targetting_tabs li.sescustom_active{
	background-color:$theme_list_background_color;
	border-bottom:2px solid $theme_button_background_color;
}
/*ADS LISTING CSS START HERE*/
.sescmads_ads_listing{
	display:flex;
	flex-wrap:wrap;
	margin:0 -5px;
	position:relative;
}
.sescmads_ads_listing_item{
	width:300px;
	border-width:1px;
	margin:5px;
}
/*Display Ad*/
.sescmads_display_ad{
	width:100%;
}
.sescmads_display_ad ._header{
	padding:10px;
	position:relative;
}
.sescmads_display_ad ._header ._img{
	float:left;
	margin-right:10px;
}
.sescmads_display_ad ._header ._img img{
	border-width:0;
	height:48px;
	vertical-align:middle;
	width:48px;
}
.sescmads_display_ad ._header ._cont{
	overflow:hidden;
	display:block;
	padding-right:20px;
}
.sescmads_display_ad ._header ._cont ._title{
	font-weight:bold;
	white-space:nowrap;
	overflow:hidden;
	text-overflow:ellipsis;
}
.sescmads_display_ad ._header ._cont ._txt{
	font-size:12px;
	margin-top:5px;
}
.sescmads_display_ad ._header ._optionbtn{
	position:absolute;
	right:10px;
	top:10px;
}
.sescmads_display_ad ._header ._optionbtn > a{
	display:block;
}
.sescmads_display_ad ._header ._optionbtn > a > i{
	font-size:16px;
	line-height:15px;
	height:15px;
	margin:2px;
	text-align:center;
	width:15px;
}
.sescmads_display_ad ._header ._optionbtn > a:hover,
.sescmads_display_ad ._header ._optionbtn > a.showpulldown{
	background-color:$theme_button_background_color;
	color:$theme_button_font_color;
}
.sescmads_display_ad ._header ._optionbtn > a:hover i,
.sescmads_display_ad ._header ._optionbtn > a.showpulldown i{
	background-color:$theme_button_background_color;
	color:$theme_button_font_color !important;
}
.sescmads_display_ad ._header ._optionbtn .sesbasic_pulldown_options{
	right:0 !important;
	margin-top:0 !important;
	border-radius:0;
}
.sescmads_display_ad ._header ._optionbtn .sesbasic_pulldown_options:before{
	display:none;
}
.sescmads_display_ad ._header ._optionbtn .sesbasic_pulldown_options{
	right:-15px;
	margin-top:5px;
}
.sescmads_display_ad ._header ._optionbtn .sesbasic_pulldown_options ul{
	list-style:none !important;
	margin:0 !important; 
}
.sescmads_display_ad_item ._badge{
	position:absolute;
	background-color:#f00;
	color:#fff;
	top:5px;
	left:5px;
	padding:0 15px;
	font-size:12px;
	line-height:25px;
}
.sescmads_display_ad_item ._img{
	height:250px;
	position:relative;
}
.sescmads_display_ad_item ._img img{
	object-fit:cover;
	height:100%;
	width:100%;
}
.sescmads_display_ad_item ._img video{
	width:100%;
	background-size:cover;
	background-position:center;
}
.sescmads_display_ad_item ._cont{
	padding: 10px;
	position: relative;
	overflow: hidden;
	display: flex;
	align-items: center;
}
.sescmads_display_ad_item ._cont ._link{
	left:0;
	top:0;
	height:100%;
	width:100%;
	position:absolute;
}
.sescmads_display_ad_item ._details{
	overflow: hidden;
	flex-grow: 1;
}
.sescmads_display_ad_item ._details ._url{
	margin-bottom:3px;
}
.sescmads_display_ad_item ._details ._headline{
	font-weight:bold;
	white-space:nowrap;
	overflow:hidden;
	text-overflow:ellipsis;
}
.sescmads_display_ad_item ._details ._des{
	max-height:40px;
	line-height:20px;
	margin-top:3px;
}
.sescmads_display_ad_item._more ._details > div{
	text-align:center;
}
.sescmads_display_ad_item._more ._img img{
	object-fit:none;
	background-color:#f3f3f3;
}
.sescmads_display_ad_item ._button{
	margin-left:15px;
}
.sescmads_display_ad_item ._button a{
	white-space: nowrap;
	border-radius: 0;
	background: rgba(0, 0, 0, .02);
	text-align:center;
}
.sescmads_display_ad_carousel{
	overflow:hidden;
	position:relative;
}
.sescmads_display_ad_carousel .owl-stage{
	display:flex;
	flex-wrap:wrap;
}
.sescmads_display_ad_carousel .owl-nav > div{
	background-color:$theme_button_background_color;
	color:$theme_button_font_color;
	cursor:pointer;
	position:absolute;
	top:120px;
	height:25px;
	width:20px;
  display:block !important;
}
.sescmads_display_ad_carousel .owl-nav > div i{
	position:absolute;
	left:50%;
	top:50%;
	transform:translate(-50%,-50%);
}
.sescmads_display_ad_carousel .owl-nav > div.owl-next{
	right:0;
}
/*Hidden Ad*/
.sescmads_hidden_ad{
	padding:10px;
}
.sescmads_hidden_ad ._ad_hidden_head{
	border-bottom-width:1px;
	padding-bottom:10px;
	margin-bottom:10px;
}
.sescmads_hidden_ad ._ad_hidden_head .floatL{
	font-weight:bold;
}
.sescmads_hidden_ad ._ad_hidden_head .floatR{
	font-size:12px;
	margin-top:2px;
}
.sescmads_hidden_ad p{
	font-size:90%;
	margin-bottom:10px;
}
.sescmads_hidden_ad ._ad_hidden_options > span{
	display:block;
	font-size:90%;
	margin-bottom:5px;
}
.sescmads_hidden_ad ._ad_hidden_options > span textarea{
	max-width:95%;
	margin-top:10px;
}
.sescmads_ads_listing_item .sescomm_report_success > span{
	text-align:center;
	display:block;
}
.sescmads_ads_listing_item .sescomm_report_success{
	padding:15px;
}
.sescmads_ads_listing_item .sescomm_report_success .success_img{
	margin-bottom:10px;
}
.sescmads_ads_listing_item .sescomm_report_success .success_img img{
	max-width:80px;
}
/*Left - Right Column*/
.layout_left .sescmads_ads_listing,
.layout_right .sescmads_ads_listing{
	margin:0;
}
/*Placed in feed*/
.sesact_feed ul.feed li.sescmads_ads_listing_item.sesact_pinfeed{
	background:none !important;
	border-width:0;
	padding:0 !important;
	width:100% !important;
	margin:0 !important;
}
.sesact_feed ul.feed > li .sesact_show_ads{
	display:flex;
	flex-wrap:wrap;
  margin:0 -5px;
}
.sesact_feed ul.feed li.sescmads_ads_listing_item{
	background-color:$theme_list_background_color;
	border-width:1px;
  margin:0 5px;
}
.sesact_feed ul.feed > li .sesact_show_ads_1 li.sescmads_ads_listing_item .sescmads_display_ad_item ._img{
  height:350px;
}
.sesact_feed ul.feed > li .sesact_show_ads_1 li.sescmads_ads_listing_item .sescmads_display_ad_item ._video{
	height: auto;
}
.sesact_feed ul.feed > li .sesact_show_ads_1 .sescmads_display_ad_carousel .owl-nav > div{
  top:140px;
}
.sesact_feed ul.feed > li .sesact_show_ads_1 li.sescmads_ads_listing_item .sescmads_display_ad_item ._cont{
	background-color:$theme_list_background_color_hover;
	background-image:url(~/application/modules/Sescommunityads/externals/images/transprant-bg.png);
}
.sesact_feed ul.feed > li .sesact_show_ads_1 li.sescmads_ads_listing_item:hover .sescmads_display_ad_item ._cont{
	background-image:none;
}
.sesact_feed ul.feed > li .sesact_show_ads_1 li.sescmads_ads_listing_item .sescmads_display_ad_item ._cont ._button a{
	background-color:$theme_list_background_color;
}
.sesact_feed ul.feed > li .sesact_show_ads_1 li.sescmads_ads_listing_item .sescmads_display_ad_item ._details ._headline{
	font-size:130%;
}
.sesact_feed ul.feed > li .sesact_show_ads_3 li.sescmads_ads_listing_item .sescmads_display_ad_item ._img{
	height:180px;
}
.sesact_feed ul.feed > li .sesact_show_ads_3 .sescmads_display_ad_carousel .owl-nav > div{
  top:80px;
}
/*ADS LISTING CSS END HERE*/
@media only screen and (max-width:480px){
	.sesact_feed ul.feed > li .sesact_show_ads_2,
	.sesact_feed ul.feed > li .sesact_show_ads_3{
		display:block;
		white-space:nowrap;
		overflow:auto;
	}
	.sesact_feed ul.feed > li .sesact_show_ads_2 li.sescmads_ads_listing_item,
	.sesact_feed ul.feed > li .sesact_show_ads_3 li.sescmads_ads_listing_item{
		float:none;
		width:100% !IMPORTANT;
		display:inline-block;
		vertical-align:top;
	}
	.sesact_feed ul.feed > li .sesact_show_ads_1 li.sescmads_ads_listing_item{
		display:inline-block;
		width:100%;
	}
	.sesact_feed ul.feed > li .sesact_show_ads_1 li.sescmads_ads_listing_item .sescmads_display_ad_item ._img{
		height:180px;
	}
	.sesact_feed ul.feed > li .sesact_show_ads_1 .sescmads_display_ad_carousel .owl-nav > div{
		top:80px;
	}
}
/*HELP CENTER CSS STARTS*/
.sescmads_help_center ._container{
	max-width:$theme_content_width;
}
.sescmads_help_center .head_banner h2{
	color:$theme_list_background_color;
}
.sescmads_help_center a.read_faq,
.sescmads_help_center a.explore_ad{
	border-color:$theme_list_background_color;
	color:$theme_list_background_color;
}
.sescmads_help_center a.read_faq:hover,
.sescmads_help_center a.explore_ad:hover{
	border-color:$theme_button_background_color_hover;
	background-color:$theme_button_background_color_hover;
}
.sescmads_help_center a.create_ad{
	border-color:$theme_list_background_color;
	color:$theme_list_background_color;
}
.sescmads_help_center a.create_ad:hover{
	background-color:$theme_list_background_color;
	color:$theme_font_color;
}
.sescmads_help_center .visibility_of_ads .match_advertisement{
	background-color:$theme_list_background_color;
}
.sescmads_help_center .faq_item,
.sescmads_help_center .how_it_works,
.sescmads_help_center ._head_con,
.sescmads_help_center .visibility_of_ads .match_bottom{
	background:$theme_list_background_color;
}
.sescmads_help_center .help_center_footer p,
.sescmads_help_center .help_center_footer .help_footer_btn span{
	color:$theme_list_background_color;
}
.sescmads_help_center .visibility_of_ads .match_bottom p{
	color:$theme_font_color_light;
}
/*Short Code Ads*/
.sescmads_shortcode_ads_display .sescmads_ads_listing{
	list-style: none !important;
	margin: 20px 0 !important;
	display: block;
	padding: 0 !important;
}
.sescmads_shortcode_ads_display .sescmads_display_ad ._header ._img img{
	height:48px !important;
	width:48px !important;
	margin:0 !important;
}
.sescmads_shortcode_ads_display .sescmads_display_ad_item ._img img{
	height:100% !important;
	width:100% !important;
	margin:0 !important;
}
.sescmads_shortcode_ads_display .sescmads_ads_listing_item{
	margin:0 auto;
}
.sescmads_shortcode_ads_display .sescmads_display_ad_item ._details ._url,
.sescmads_shortcode_ads_display .sescmads_display_ad_item ._details ._des{
	font-weight:normal;
}