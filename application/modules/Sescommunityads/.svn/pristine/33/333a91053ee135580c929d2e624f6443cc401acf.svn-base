.sescmads_help_center .faq_blk{
	padding:50px 0;
	float:left;
	width:100%;
	background-image:url(../images/transprant-bg.png);
}
.sescmads_help_center .faq_item{
	margin:10px;
	width:33%;
	box-shadow:0 0 10px #f1f1f1;
	padding:20px;
}
.sescmads_help_center .faq_blk .align-items{
	display:flex;
	justify-content:space-between;	
}
.sescmads_help_center .faq_blk .faq_item h4{
	text-align:center;
	text-transform:uppercase;
	font-weight:bold;
	margin:10px 0;
	letter-spacing:0.5px;
	font-size:115%;
}
.sescmads_help_center .faq_blk .faq_item img{
	width:100px;
	height:auto;
	margin:0 auto;
	display:block;
}
.sescmads_help_center .faq_blk .faq_item p{
	text-align:center;
	font-size:110%;
	margin-bottom:10px;
  line-height:130%;
}
.sescmads_help_center .help_center_footer{
	background:url(../images/footer-banner.jpg) no-repeat center center;
	height:200px;
	position:relative;
	float:left;
	width:100%;
	background-size:cover;
}
.sescmads_help_center .help_center_footer .footer_desc{
	position:absolute;
	top:50%;
	transform:translateY(-50%);
	left:0;
	right:0;
}
.sescmads_help_center .help_center_footer p{
	font-size:200%;
	font-weight:300;
	padding:0 50px;
	text-align:center;
}
.sescmads_help_center .help_center_footer .help_footer_btn{
	text-align:center;
	margin:30px auto 0 auto;
}
.sescmads_help_center .help_center_footer a.explore_ad{
	border-width:2px;
	margin-right:10px;
	border-radius:25px;
	padding:10px 30px;
	transition:all 0.4s ease;
}
.sescmads_help_center .help_center_footer .help_footer_btn span{
	padding:0 5px;
}
