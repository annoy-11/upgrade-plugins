}
.sescmads_campaigns_details .make-payment .paymnt_b + .paymnt_b{
	margin-top:10px;
}
.sescmads_campaigns_details ._option{
	position:relative;
}
.sescmads_campaigns_details ._option > a{
  padding:0 10px;
	line-height:28px;
	background:transparent;
}
.sescmads_campaigns_details .showpulldown + .sesbasic_pulldown_options{
	right:0;
  margin-top:5px;
}
.sescmads_campaigns_details .showpulldown + .sesbasic_pulldown_options ul{
	overflow:hidden;
}
.sescmads_campaigns_details .showpulldown + .sesbasic_pulldown_options li a{
	padding-left:8px !important;
}
.sescmads_camp_inner .ad_information{
	flex-basis:100%;
}
.sescmads_camp_inner .ad_information input[type=checkbox]{
	margin-right:10px;
	float:left;
}
.sescmads_campaigns_details .ad_status{
	overflow:hidden;
}
.sescmads_campaigns_details .ad_status ._title{
	margin-bottom:5px;
}
.sescmads_campaigns_details .ad_status ._title a{
	font-weight:bold;
	font-size:15px;
	text-transform:capitalize;
	letter-spacing:.5px;
	width:100%;
}
.sescmads_campaigns_details .ad_status ._info{
	display:flex;
	flex-wrap:wrap;
}
.sescmads_campaigns_details .ad_status ._info span{
  font-weight:500;
	margin-top:5px;
	margin-bottom:5px;
	width:50%;
}
.sescmads_campaigns_details .ad_status ._info span._label{
	font-weight:bold;
}
.sescmads_campaign_view_dlt_btn{
	margin:15px 0;
}
/*MY CAMPAIGN MANAGE ADS END HERE*/
/*STATS GRAPH START HERE*/
.sescmads_stats_graph{
	border-width:1px;
}
.sescmads_stats_graph .sescmads_stats_graph_box,
.sescmads_stats_graph .sesbasic_loading_container{
	height:300px;
}
.sescmads_stats_graph .sesbasic_filter_tabs{
	border-width:0 0 1px;
}
.sescmads_stats_graph .sesbasic_filter_tabs > div{
	float:left;
	display:flex;
}
.sescmads_stats_graph .sesbasic_filter_tabs > div *{
	margin-right:5px;
}
.sescmads_stats_graph .sesbasic_filter_tabs > ul{
	float:right;
}
/*STATS GRAPH END HERE*/
/*AD VIEW PAGE START HERE*/
.sescmads_view_breadcrumbs{
	margin-bottom:10px;
	border-bottom-width:1px;
	padding-bottom:10px;
}
.sescmads_view_header ._adtitle{
	float:left;
	font-size:20px;
	font-weight:600;
}
.sescmads_view_header ._right{
	float:right;
}
.sescmads_view_header ._right .layout_sescommunityads_payment_status{
	margin:0 !important;
}
.sescmads_view_header ._right ._payment{
	float:left;
}
.sescmads_view_header ._right ._payment{
	float:left;
}
.sescmads_view_header ._right ._payment .paymnt_b{
	line-height:30px;
}
.sescmads_view_header ._right ._payment .paymnt_b span + span{
	font-weight:bold;
}
.sescmads_view_header ._right ._payment .sescmads_payment_btn{
	padding:0 20px;
}
.sescmads_view_header ._right ._option{
	float:right;
	margin-left:10px;
	position:relative;
}
.sescmads_view_header ._right ._option > a{
	padding:0 10px;
	line-height:28px;
	background:transparent;
}
.sescmads_view_header ._right .showpulldown + .sesbasic_pulldown_options{
	right:0;
	margin-top:5px;
	width:100px;
}
.sescmads_view_main{
	margin-top:20px;
}
.sescmads_view_content{
	float:left;
	width:50%;
}
.sescmads_view_content ._adinfo > div{
	margin-bottom:15px;
}
.sescmads_view_content ._adinfo > div span:nth-child(1){
	font-weight:bold;
	float:left;
	width:150px;
}
.sescmads_view_content ._adinfo > div span + span{
	overflow:hidden;
	display:block;
}
.sescmads_view_content ._adstats{
	margin-top:20px;
	display:flex;
	border-top-width:1px;
	padding-top:20px;
}
.sescmads_view_content ._adstats > div{
	width:25%;
	text-align:center;
}
.sescmads_view_content ._adstats > div > span{
	display:block;
}
.sescmads_view_content ._adstats > div > span:nth-child(1){
	font-weight:bold;
	font-size:120%;
}
.sescmads_view_preview{
	float:left;
	width:50%;
}
.sescmads_view_preview .sescmads_create_preview{
	margin:0 auto;
	position:relative;
}
.sescmads_view_preview .sescmads_create_preview .sescmads_ads_listing_item{
	width:100%;
}
.sescmads_view_preview .comments{
	display:none !important;
}
.sescmads_view_preview .sescmads_create_preview:after{
	content:"";
	position:absolute;
	left:0;
	top:0;
	height:100%;
	width:100%;
}
/*PACKAGES CSS START HERE*/
.sescmads_packages_main{
	padding:50px 20px;
	margin-bottom:40px;
}
.sescmads_packages_main ._header{
	text-align:center;
	margin-bottom:25px;
}
.sescmads_packages_main ._header h2{
	font-size:30px;
	font-weight:bold;
	letter-spacing:normal;
	margin:10px;
	padding:0;
}
.sescmads_packages_main ._header p{
	font-size:17px;
	margin-top:10px;
}
/*Vertical*/
.sescommunityads_packages_table{
  display:flex;
  flex-wrap:wrap;
}
.sescmads_packages_table_item{
  width:25%;
  margin:10px 0;
  padding:0 7px;
}
.sescmads_packages_table_item > section{
	border-radius:3px;
	border-bottom-width:2px;
	height:100%;
}
.sescmads_packages_table_item ._title{
  padding:15px;
}
.sescmads_packages_table_item ._title h5{
	font-size:20px;
	line-height:30px;
	margin-bottom:0;
	font-weight:normal;
	text-align:center;
}
.sescmads_packages_table_item ._title span{
	display:block;
	font-size:20px;
	line-height:30px;
	margin-bottom:0;
	font-weight:700;
	text-align:center;
}
.sescmads_packages_table_item ._cont{
	border-top-width:1px;
	padding:10px;
}
.sescmads_packages_table_item .package_capabilities li{
	border-bottom-width:1px;
	padding-bottom:10px;
	margin-bottom:10px;
	font-size:90%;
}
.sescmads_packages_table_item .package_capabilities li span._label{
	float:left;
	font-weight:bold;
}
.sescmads_packages_table_item .package_capabilities li span._value{
	padding-left:10px;
	overflow:hidden;
	display:block;
	text-align:right;
}
.sescmads_packages_table_item .package_des{
	font-size:90%;
	line-height:150%;
	font-style:italic;
	text-align:center;
}
.sescmads_packages_table_item ._btn{
  padding:10px 10px 10px;
}
.sescmads_packages_table_item ._btn a{
	display:block;
	padding:15px;
	text-align:center;
	border-radius:5px;
	font-size:13px;
	font-weight:600;
	text-transform:uppercase;
}
.sescmads_packages_table_item.active > section{
  transform:scale(1.03);
}
.sescmads_packages_table_item > section{
  border-radius:3px;
  border-bottom-width:2px;
  height:100%;
  box-shadow:0 0 5px #e2e2e2;
}
.sescmads_packages_table_item .package_capabilities li span._value ._icon{
	display:inline-block;
	height:16px;
	width:16px;
}
.sescmads_packages_table_item .package_capabilities li span._value ._yes{
  background-image:url(../images/tick-green.png);
}
.sescmads_packages_table_item .package_capabilities li span._value ._no{
  background-image:url(../images/cross.png);
}
/*Horrizonal List*/
.sescmads_packages_list{
	display:flex;
	flex-wrap:wrap;	
}
.sescmads_packages_list_item{
	width:100%;
	margin-top:20px;
}
.sescmads_packages_list_item > section{
	border-radius:3px;
	box-shadow:0 0 5px #e2e2e2;
}
.sescmads_packages_list_item ._top{
	padding:0 20px 25px;
}
.sescmads_packages_list_item ._top ._title{
	float:left;
}
.sescmads_packages_list_item ._top ._title h5{
	font-size:20px;
	line-height:50px;
	padding:0 30px;
	min-width:150px;
	text-align:center;
}
.sescmads_packages_list_item ._top ._price{
	float:right;
	padding-top:10px;
}
.sescmads_packages_list_item ._top ._price span{
	font-size:30px;
	margin-bottom:0;
	vertical-align:middle;
}
.sescmads_packages_list_item ._cont{
	padding:0 20px 10px;
	display:table;
	width:100%;
}
.sescmads_packages_list_item ._cont ._features{
	display:flex;
	flex-wrap:wrap;
}
.sescmads_packages_list_item .package_capabilities > div{
	float:left;
	margin-bottom:10px;
	width:33.33%;
	padding-right:20px;
}
.sescmads_packages_list_item .package_capabilities > div > div{
	overflow:hidden;
	border-bottom-width:1px;
	padding-bottom:10px;
	font-size:90%;
}
.sescmads_packages_list_item .package_capabilities > div:nth-last-child(1){
	border-bottom-width:0;
}
.sescmads_packages_list_item .package_capabilities span._label{
	float:left;
	font-weight:bold;
}
.sescmads_packages_list_item .package_capabilities span._value{
	overflow:hidden;
	display:block;
	text-align:right;
	padding-left:10px;
}
.sescmads_packages_list_item .package_capabilities span._value ._icon{
	display:inline-block;
	height:16px;
	width:16px;
}
.sescmads_packages_list_item .package_capabilities span._value ._yes{
	background-image:url(../images/tick-green.png);
}
.sescmads_packages_list_item .package_capabilities span._value ._no{
	background-image:url(../images/cross.png);
}
.sescmads_packages_list_item ._cont ._des{
	font-size:90%;
	display:table-cell;
	width:25%;
	padding:15px;
	background-color:rgba(0, 0, 0, .05);
	line-height:22px;
	vertical-align:top;
}
.sescmads_packages_list_item ._btns{
	padding:10px 20px;
	text-align:right;
	border-top-width:1px;
}
.sescmads_packages_list_item ._btns a{
	display:inline-block;
	padding:13px 30px;
	text-align:center;
	border-radius:5px;
	font-size:13px;
	font-weight:600;
	text-transform:uppercase;
	letter-spacing:2px;
}
.sescmads_packages_list_item ._btns ._left{
	float:left;
}
.sescommunityads_packages_cancel_btn{
	background-color:#ca1616;
	color:#fff !important;
}
.sescmads_packages_list_item ._btns ._left > a{
	margin-right:10px;
}
.sescmads_packages_list_item ._btns ._left span{
	display:inline-block;
	vertical-align:middle;
}
.sescmads_packages_list_item ._btns ._right{
	float:right;
}
/*PACKAGES CSS END HERE*/

/************************CREATE AN AD CSS START HERE*************************/
/*Choose Ad Type Start*/
.sescmads_select_ads_container{
	padding:50px 20px;
}
.sescmads_select_ads_container > ._head{
	font-size:24px;
	margin-bottom:5px;
	font-weight:600;
}
.sescmads_select_ads{
	margin:20px auto 0;
	width:650px;
}
.sescmads_select_ads ul li{
	border-width:1px;
	border-radius:3px;
	overflow:hidden;
	margin-top:10px;
	transition:all .5s ease;
}
.sescmads_select_ads ul li:hover{
	transform:translateY(-5px);
	transition:all .5s ease;
}
.sescmads_select_ads ul li a{
	display:flex;
	align-items:center;
	width:100%;
}
.sescmads_select_ads ul li ._thumb img{
	width:160px;
}
.sescmads_select_ads ul li *{
	transition:all .5s ease;
}
.sescmads_select_ads ul li ._cont{
	padding:0 20px;
	width:100%;
}
.sescmads_select_ads ul li ._title{
	font-weight:bold;
	margin-bottom:5px;
}
.sescmads_select_ads ul li ._arrow{
	width:50px;
	text-align:center;
	font-size:17px;
}
.sescmads_select_ads ul li ._arrow i{
	border:1px solid;
	width:18px;
	height:18px;
	line-height:18px;
	border-radius:100px;
	font-size:13px;
	text-align:center;
}
/*Choose Ad Type End*/
/*Container*/
.sescmads_create_container{
	padding:10px;
	position:relative;
}
.sescmads_create_container .sescmads_create_container{
	padding:0;
	background:none;
}
.sescmads_create_container .sesbasic_loading_cont_overlay{
	margin:0;
}
.sescmads_create_step{
	border-radius:5px;
	margin-bottom:15px;
	overflow:hidden;
}
.sescmads_create_step_header{
	border-bottom-width:1px;
	padding:10px;
	font-weight:bold;
	font-size:17px;
}
.sescmads_create_step_header button{
	padding:0 !important;
	background:none !important;
	box-shadow:none !important;
}
.sescmads_create_step_footer{
	border-top-width:1px;
	padding:10px;
}
.sescmads_create_final_step_footer{
	width:100%;
}
.sescmads_create_step_footer > div + div{
	margin-left:10px;
}
.sescmads_create_step_footer a{
	padding:0 20px;
	border-radius:3px;
	display:inline-block;
	line-height:34px;
	vertical-align:middle;
}
.sescmads_create_step_footer a:hover{
	box-shadow:0 4px 10px 0 rgba(0,0,0,0.2), 0 4px 20px 0 rgba(0,0,0,0.19);
}
/*Select Campaign*/
.sescmads_create_campaign{
	padding:15px;
}
.sescmads_create_campaign_field{
	margin-bottom:15px;
}
.sescmads_create_campaign_field:nth-last-child(1){
	margin-bottom:0;
}
.sescmads_create_campaign_label{
	float:left;
	margin-right:15px;
	width:160px;
	padding-top:5px;
}
.sescmads_create_campaign_label .required:after{
	content:' *';
	color:#f00;
}
.sescmads_create_campaign_element{
	overflow:hidden;
}
.sescmads_create_campaign_element input,
.sescmads_create_campaign_element select{
	max-width:300px;
	width:100%;
}
.sescmads_create_hint{
	margin-top:5px;
	display:block;
	font-size:12px;
}
/*Carousel Content*/
.sescmads_create_cont_nav{
	margin-bottom:20px;
	display:flex;
	flex-wrap:wrap;
}
.sescmads_create_cont_nav li{
	display:inline-block;
	vertical-align:middle;
	position:relative;
}
.sescmads_create_cont_nav_add{
	margin-left:5px;
}
.sescmads_create_cont_nav li a,
.sescmads_create_cont_nav_add button{
	height:30px;
	width:30px;
	display:block;
	line-height:30px;
	text-align:center;
	padding:0;
}
.sescmads_create_cont_nav li.sescomm_active:after{
	content:"";
	border-left:8px solid transparent;
	border-right:8px solid transparent;
	height:0;
	left:5px;
	margin:0 auto;
	position:absolute;
	width:0;
	z-index:1;
	bottom:-20px;
}
.sescmads_create_carousel_item{
	padding:15px;
	margin-bottom:15px;
}
.sescmads_create_carousel_item_remove{
	text-align:right;
	margin-bottom:10px;
}
/*Select Post*/
.sescmads_select_post{
	padding:15px;
	max-height:400px;
	overflow:auto;
}
.sescmads_select_post li{
	border-width:1px;
	border-radius:3px;
	overflow:hidden;
	margin-bottom:10px;
	padding:10px;
	background-image:url(../images/transprant-bg.png);
}
.sescmads_select_post li ._postitem_cont ._thumb{
	width:80px;
	height:80px;
}
.sescmads_select_post li ._postitem_cont ._thumb img{
	height:100%;
	width:100%;
	object-fit:cover;
}
.sescmads_select_post li ._options{
	margin:0 10px;
	letter-spacing:.2px;
	flex-basis:50%;
}
.sescmads_select_post li ._options ._shortdes{
	height:40px;
	line-height:20px;
	-webkit-line-clamp:2;
	overflow:hidden;
	text-overflow:ellipsis;
	font-size:14px;
}
.sescmads_select_post li ._other_options > div > span{
  margin-left:4px;
}
.sescmads_select_post li ._options ._publish_date{
	font-size:12px;
	margin-top:10px;
}
.sescmads_select_post li ._other_options ._reaction_count span i{
	width:20px;
	height:20px;
	background-repeat:no-repeat;
	background-size:100%;
	display:inline-block;
	vertical-align:middle;
	margin:0 -4px;
	border:2px solid white;
	border-radius:100%;
}
.sescmads_select_post li ._other_options > ._reaction_count,
.sescmads_select_post li ._other_options > ._comment_count,
.sescmads_select_post li ._other_options > ._share_count, 
.sescmads_select_post li ._boost_btn{
	flex-basis:100%;
	text-align:center;
}
.sescmads_select_post li ._other_options, ._postitem_main{
	display:flex;
	align-items:center;
	flex-basis:100%;
}
.sescmads_select_post li ._other_options > ._boost_btn > button{
  padding:10px 12px;
	color:white;
	border-radius:2px;
}
/*Select Format */
.sescmads_choose_format{
	padding:15px;
}
.sescmads_choose_format > ul{
	display:flex;
	flex-wrap:wrap;
	align-items:center;
	justify-content:center;
	margin:0 -10px;
}
.sescmads_choose_format > ul > li{
	text-align:center;
	border-radius:5px;
	flex-grow:1;
	padding:0 10px;
}
.sescmads_choose_format > ul > li article{
	border-width:1px;
	border-radius:10px;
	position:relative;
}
.sescmads_choose_format > ul > li article ._tick{
	border-radius:50%;
	border-width:1px;
	position:absolute;
	left:15px;
	top:15px;
	height:20px;
	width:20px;
	color:transparent;
}
.sescmads_choose_format > ul > li article ._tick:before{
	position:absolute;
	left:50%;
	top:50%;
	transform:translate(-50%,-50%);
}
.sescmads_choose_format > ul > li article > input{
  display:none;
}
.sescmads_choose_format > ul > li article label{
	padding:30px;
	position:relative;
}
.sescmads_choose_format > ul > li .format_img{
	overflow:hidden;
	margin:auto;
	margin-bottom:10px;
	display:block;
}
