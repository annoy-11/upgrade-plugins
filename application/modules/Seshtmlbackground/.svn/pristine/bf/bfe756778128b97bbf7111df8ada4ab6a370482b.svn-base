.cd-logo{
	margin:10px 0 0;
}
.cd-logo img {
  max-height: 50px;
}
.cd-primary-nav {
  margin: 20px 0 0 50px;
}
.cd-mini-menu > ul > li > a, .cd-mini-menu .sm_minimenu_links > li > a{
	font-size: 100%;
	padding:5px 0;
}
.cd-mini-menu .core_mini_signup, .cd-mini-menu .sm_minimenu_links > li > a.signup-link,.cd-mini-menu > ul > li .user_auth_link{
  border-radius: 4px;
	padding:4px 15px !important;
}
.cd-mini-menu > ul > li .core_mini_notification:before {
	content: "\f0a2";
	font-family: "FontAwesome";
	margin-right: 5px;
}
.cd-mini-menu > ul > li .core_mini_friends:before {
	content: "\f234";
	font-family: "FontAwesome";
	margin-right: 5px;
}
.cd-hero-slider h2,.cd-hero-slider p{
	/*width: 80%;*/
	max-width: 100%;
	margin: 10px 5px;
}
.cd-hero-slider h2{
	font-size: 42px !important;
	font-weight: 100 !important;
	text-transform: capitalize;
}
.cd-hero-slider .form-in-right .cd-half-width:nth-of-type(3),.cd-hero-slider .form-in-left .cd-half-width:nth-of-type(2){
	text-align:left;
}
.cd-hero-slider .form-in-right .cd-half-width:nth-of-type(2),.cd-hero-slider .form-in-left .cd-half-width:nth-of-type(3){
	text-align:right;
}
.cd-hero-slider .cut_rectangle:before{
	content: "";
	position: absolute;
	top: auto;
	left: -7%;
	right: auto;
	bottom: auto;
	height: 100%;
	width: 55%;
	transform: skewX(-15deg);
	opacity:.5;
}
.cd-hero-slider .cut_rectangle .cd-half-width:nth-of-type(3){
	width:45%;
}
.cd-hero-slider .cut_rectangle .seshtmlbackground_form{
	position:relative;
	background:transparent;
	padding:0 10px;
}
.cd-hero-slider .cut_rectangle .cd-signupform-tabs{
	background:transparent;
	padding-left:0;
	padding-right:0;
	padding-bottom: 8px;
}
.cd-hero-slider .cut_rectangle .seshtmlbackground_form .form-element{
	margin-bottom:15px;
}
.cut_rectangle .cd-signupform-tabs a{
	margin-right:0;
	padding: 10px 15px;
	border-bottom:0;
}
.cut_rectangle .cd-signupform-tabs a#signup_seshtml_btn_4 {
  margin-left: -4px;
}
.cd-hero-slider .cut_rectangle .seshtmlbackground_form #submit-wrapper {
  float: none !important;
}
.cd-hero-slider .cut_rectangle .seshtmlbackground_form #submit-element{
	text-align:center;
	padding: 4px;
  border-radius: 0;
}
.cd-hero-slider .cut_rectangle .seshtmlbackground_form:before{
	content: "";
	position: absolute;
	top: 0;
	left: 0;
	right: 0;
	bottom: 0;
	width: 100%;
	height: 100%;
	opacity:.4;
}
.form-in-left.cut_rectangle:before {
	right: -7%;
	left: auto;
	width:60%;
}
.form-in-left.cut_rectangle .cd-half-width{
	text-align:center !important;
}
.form-in-left.cut_rectangle h2,.form-in-left.cut_rectangle p{
	width: 90%;
  margin: auto;
}
.cd-loginbox .global_form_box input[type="email"], .cd-loginbox .global_form_box input[type="password"]{
	padding: 12px 5% 12px 30px;
}
.cd-loginbox .global_form_box .form-wrapper:nth-child(1) .form-element:before, .cd-loginbox .global_form_box #password-element:before,.cd-signupform .global_form_box #email-element:before, .cd-signupform .global_form_box .form-wrapper:nth-child(1) .form-element:before, .cd-signupform .global_form_box .form-wrapper:nth-child(2) .form-element:before, .cd-signupform .global_form_box #password-element:before, .cd-signupform .global_form_box #passconf-element:before, .cd-signupform .global_form_box #username-element:before{
	font-size: 16px;
  top: 11px;
}
.cd-signupform .global_form_box input[type="text"], .cd-signupform .global_form_box input[type="email"], .cd-signupform .global_form_box input[type="password"],.cd-signupform .global_form_box select{
  padding: 12px 3% 12px 30px;
}
.cd-signupform .global_form_box select{
  padding:10px 3%;
}
.cd-full-width .cd-hero-slider h2, .cd-full-width .cd-hero-slider p {
  width: 100% !important;
}
.cd-hero-slider li{
  overflow:hidden;
}
.cd-full-width .cd-hero-slider .cut_rectangle:before{
  display:none;
}
.cd-hero-slider .form-in-left .cd-half-width:nth-of-type(2){
	left:5%;
	right:auto !important;
}
.cd-hero-slider .form-in-left .cd-half-width:nth-of-type(3){
  float: right;
  position: relative;
}
.cd-hero-slider .form-in-right .cd-half-width:nth-of-type(3){
	left:5%;
}
.cd-hero-slider .form-in-right .cd-half-width:nth-of-type(2){
	right: -20%;
	left:auto !important;
}
.cd-hero-slider .cd-full-width{
	text-align:center;
}
.cd-hero-slider .cd-full-width h2,.cd-hero-slider .cd-full-width p{
	width:100% !important;
}